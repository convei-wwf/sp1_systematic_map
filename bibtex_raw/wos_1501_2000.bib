@article{ WOS:000777609900001,
Author = {Tellman, Beth and Lall, Upmanu and Islam, A. K. M. Saiful and Bhuyan, Md
   Ariffuzaman},
Title = {Regional Index Insurance Using Satellite-Based Fractional Flooded Area},

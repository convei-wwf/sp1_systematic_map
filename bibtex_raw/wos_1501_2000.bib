@article{ WOS:000219230500004,
Author = {Kushwaha, Deendayal and Janagam, Sridhar and Trivedi, Neeta},
Title = {Compute-Efficient Geo-Localization of Targets from UAV Videos: Real-Time
   Processing in Unknown Territory},

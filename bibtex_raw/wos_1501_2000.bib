@article{ WOS:000916331500001,
Author = {Quang, Nguyen Hong and Dinh, Nguyen Tran and Dien, Nguyen Tran and Son,
   Le Thanh},
Title = {Calibration of Sentinel-2 Surface Reflectance for Water Quality
   Modelling in Binh Dinh's Coastal Zone of Vietnam},
Journal = {SUSTAINABILITY},

@article{ WOS:000809324200001,
Author = {John, Susan and Varghese, A. O.},
Title = {Analysis of support vector machine and maximum likelihood classifiers in
   land cover classification using Sentinel-2 images},
Journal = {PROCEEDINGS OF THE INDIAN NATIONAL SCIENCE ACADEMY},
Year = {2022},
Volume = {88},

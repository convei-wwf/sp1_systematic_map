@article{ WOS:000704695300006,
Author = {Choudhary, Komal and Shi, Wenzhong and Boori, Mukesh Singh and Corgne,
   Samuel},
Title = {Agriculture Phenology Monitoring Using NDVI Time Series Based on Remote
   Sensing Satellites: A Case Study of Guangdong, China},
Journal = {OPTICAL MEMORY AND NEURAL NETWORKS},

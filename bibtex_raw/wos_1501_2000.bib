@article{ WOS:000365923400032,
Author = {Liu, Jiaming and Zhang, Yanjun and Yuan, Di and Song, Xingyuan},
Title = {Empirical Estimation of Total Nitrogen and Total Phosphorus
   Concentration of Urban Water Bodies in China Using High Resolution
   IKONOS Multispectral Imagery},

Title = {Development of High-performance Graphene-HgCdTe Detector Technology for
   Mid-wave Infrared Applications},
Booktitle = {IMAGE SENSING TECHNOLOGIES: MATERIALS, DEVICES, SYSTEMS, AND
   APPLICATIONS IX},

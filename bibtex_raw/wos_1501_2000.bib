@article{ WOS:000377215200030,
Author = {Selgrath, Jennifer C. and Roelfsema, Chris and Gergel, Sarah E. and
   Vincent, Amanda C. J.},
Title = {Mapping for coral reef conservation: comparing the value of
   participatory and remote sensing approaches},

@article{ WOS:000283468800012,
Author = {Zhang, Jingcheng and Jin, Yan and Huang, Wenjiang and Gu, Xiaohe and
   Huang, Jingfeng and Wang, Jihua},
Title = {SPATIO-TEMPORAL ASSESSMENT OF ECOSYSTEM SERVICES VALUE ON REGIONAL SCALE},

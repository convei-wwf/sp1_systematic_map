@article{ WOS:000545205300010,
Author = {Islam, Muhammad Aminul and Anderson, Derek T. and Pinar, Anthony J. and
   Havens, Timothy C. and Scott, Grant and Keller, James M.},
Title = {Enabling Explainable Fusion in Deep Learning With Fuzzy Integral Neural
   Networks},
Journal = {IEEE TRANSACTIONS ON FUZZY SYSTEMS},

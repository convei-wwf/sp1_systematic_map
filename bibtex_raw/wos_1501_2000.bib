@article{ WOS:000375256100003,
Author = {Diaz-Delgado, R. and Carro, F. and Quiros, F. and Osuna, A. and Baena,
   M.},
Title = {Contribution from Long-Term Ecological Monitoring to research and
   management of Donana LTSER Platform},
Journal = {ECOSISTEMAS},
Year = {2016},

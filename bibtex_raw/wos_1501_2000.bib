@inproceedings{ WOS:000344548600002,
Author = {Liu, Chaoshun and Gao, Wei and Liu, Pudong and Sun, Zhibin},
Editor = {Gao, W and Chang, NB and Wang, J},
Title = {Assimilation of remote sensing data into crop growth model to improve
   the estimation of regional winter wheat yield},
Booktitle = {REMOTE SENSING AND MODELING OF ECOSYSTEMS FOR SUSTAINABILITY XI},
Series = {Proceedings of SPIE},

@inproceedings{ WOS:000259568100002,
Author = {Bastiaanssen, W. G. M. and Pelgrum, H. and Soppe, R. W. O. and Thoreson,
   B. P. and Allen, R. G. and Teixeira, A. H. de C.},
Editor = {Goodwin, I and OConnell, MG},
Title = {Thermal-infrared technology for local and regional scale irrigation
   analyses in horticultural systems},
Booktitle = {PROCEEDINGS OF THE FIFTH INTERNATIONAL SYMPOSIUM ON IRRIGATION OF
   HORTICULTURAL CROPS},

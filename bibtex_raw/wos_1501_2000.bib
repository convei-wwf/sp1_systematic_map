@article{ WOS:000428474500021,
Author = {Grimaldi, S. and Li, Y. and Walker, J. P. and Pauwels, V. R. N.},
Title = {Effective Representation of River Geometry in Hydraulic Flood Forecast
   Models},
Journal = {WATER RESOURCES RESEARCH},
Year = {2018},

@article{ WOS:000268167000010,
Author = {Schumann, Guy and Di Baldassarre, Giuliano and Bates, Paul D.},
Title = {The Utility of Spaceborne Radar to Render Flood Inundation Maps Based on
   Multialgorithm Ensembles},

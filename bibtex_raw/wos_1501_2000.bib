Title = {Assessment of groundwater potential zone using GIS-based
   multi-influencing factor (MIF), multi-criteria decision analysis (MCDA)
   and electrical resistivity survey techniques in Raipur city,
   Chhattisgarh, India},
Journal = {AQUA-WATER INFRASTRUCTURE ECOSYSTEMS AND SOCIETY},
Year = {2021},

Author = {Bouasria, Abdelkrim and Namr, Khalid Ibno and Rahimi, Abdelmejid and
   Ettachfini, El Mostafa},
Editor = {Kacprzyk, J and Balas, VE and Ezziyyani, M},
Title = {Estimate Soil Organic Matter from Remote Sensing Data by Using
   Statistical Predictive Models},
Booktitle = {ADVANCED INTELLIGENT SYSTEMS FOR SUSTAINABLE DEVELOPMENT (AI2SD'2020),
   VOL 1},
Series = {Advances in Intelligent Systems and Computing},

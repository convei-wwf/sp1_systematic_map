@article{ WOS:000387192800013,
Author = {Luo, Jun and Chen, Lei and Tang, Guo-jin},
Title = {Attitude optimization of optical space surveillance satellite to observe
   specific geostationary orbit object},
Journal = {OPTIK},
Year = {2016},
Volume = {127},
Number = {24},
Pages = {11706-11716},
Abstract = {Due to the restriction of field of view (FOV) of GEO camera, the optical
   space-based space surveillance satellite may be not able to observe some
   specific important GEO objects in time. The satellite attitude could be
   pre-adjusted based on the relative position between satellite and
   object. The available attitude adjusting angle is obtained through
   analysis of the azimuth and elevation of object to satellite. The
   attitude adjusting angle is optimized by the Powell method with the
   accessible duration as index, and the method for determining the initial
   value is provided. The results show that the optimization of attitude
   both in three axis stabilized attitude mode and quasi-inertial attitude
   mode can improve the accessible duration to 300 s or longer. This method
   can determine the adjusting angle conveniently and improve the
   capability of satellite. (C) 2016 Elsevier GmbH. All rights reserved.},

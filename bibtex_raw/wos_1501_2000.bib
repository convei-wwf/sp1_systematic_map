@article{ WOS:000433501600020,
Author = {Jing, Wenlong and Song, Jia and Zhao, Xiaodan},
Title = {Evaluation of Multiple Satellite-Based Soil Moisture Products over
   Continental US Based on In Situ Measurements},
Journal = {WATER RESOURCES MANAGEMENT},

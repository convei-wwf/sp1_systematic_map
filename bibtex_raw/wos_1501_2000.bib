@article{ WOS:000454817100424,
Author = {Wu, Wei and Sun, Xia and Wang, Xianwei and Fan, Jing and Luo, Jiancheng
   and Shen, Ying and Yang, Yingpin},
Title = {A Long Time-Series Radiometric Normalization Method for Landsat Images},

@article{ WOS:000451598900092,
Author = {Scaioni, Marco and Marsella, Maria and Crosetto, Michele and Tornatore,
   Vincenza and Wang, Jin},
Title = {Geodetic and Remote-Sensing Sensors for Dam Deformation Monitoring},
Journal = {SENSORS},
Year = {2018},

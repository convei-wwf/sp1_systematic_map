@article{ WOS:000456754600044,
Author = {Luo, Wei and Xu, Xianli and Liu, Wen and Liu, Meixian and Li, Zhenwei
   and Peng, Tao and Xu, Chaohao and Zhang, Yaohua and Zhang, Rongfei},
Title = {UAV based soil moisture remote sensing in a karst mountainous catchment},

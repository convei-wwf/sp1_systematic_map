@article{ WOS:000382096600032,
Author = {Luo Xiao-long and Tong Zhi-jun and Zhao Yun-sheng and Zhang Ji-quan},
Title = {An Identification Study on Field-Derived Spectra of Grassland
   Combustibles and Soil Based on Fractal Theory},
Journal = {SPECTROSCOPY AND SPECTRAL ANALYSIS},

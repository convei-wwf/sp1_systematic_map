@article{ WOS:000247392100022,
Author = {Ekercin, Semih},
Title = {Water quality retrievals from high resolution IKONOS multispectral
   imagery: A case study in Istanbul, Turkey},
Journal = {WATER AIR AND SOIL POLLUTION},

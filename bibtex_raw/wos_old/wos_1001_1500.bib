@article{ WOS:000249262500005,
Author = {Leung, Yee and Fung, Tung and Mi, Ju-Sheng and Wu, Wei-Zhi},
Title = {A rough set approach to the discovery of classification rules in spatial
   data},
Journal = {INTERNATIONAL JOURNAL OF GEOGRAPHICAL INFORMATION SCIENCE},

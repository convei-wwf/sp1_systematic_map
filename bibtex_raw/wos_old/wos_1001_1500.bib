@article{ WOS:001017916100001,
Author = {Ding, Lei and Qi, Cuicui and Li, Geng and Zhang, Weiqing},
Title = {TP Concentration Inversion and Pollution Sources in Nanyi Lake Based on
   Landsat 8 Data and InVEST Model},
Journal = {SUSTAINABILITY},
Year = {2023},

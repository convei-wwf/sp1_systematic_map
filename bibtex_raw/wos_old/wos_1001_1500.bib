@article{ WOS:A1996TW33400021,
Author = {ORegan, PR},
Title = {The use of contemporary information technologies for coastal research
   and management - A review},
Journal = {JOURNAL OF COASTAL RESEARCH},
Year = {1996},
Volume = {12},
Number = {1},
Pages = {192-204},
Month = {WIN},
Abstract = {The need for better monitoring and analysis of the coastal environment
   has increased dramatically in recent years, in particular along coastal
   boundaries where human activities are extensive and give rise to
   numerous adverse impacts. During the last decade, rapid advances in
   computer technology have revolutionised the processes of data collection
   and analysis within coastal research. These advances have significantly
   increased our ability to understand the complex interrelationships
   between coastal processes and to effectively manage the resources of the
   coastal zone. This paper describes aspects of four technologies which
   are at the forefront of the `information revolution', namely: remote
   sensing, Global Positioning systems (GPS), Geographic information
   systems (GIS), and the Internet. Having introduced each technology, the
   discussion focuses on their current (and potential) uses and the
   benefits which can accrue from their adoption.},

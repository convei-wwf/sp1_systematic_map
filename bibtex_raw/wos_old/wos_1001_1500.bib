@article{ WOS:000350890300017,
Author = {Talaulikar, Madhubala and Suresh, T. and Desa, Elgar and Inamdar, Arun},
Title = {Optical Closure of Apparent Optical Properties in Coastal Waters off Goa},
Journal = {JOURNAL OF THE INDIAN SOCIETY OF REMOTE SENSING},

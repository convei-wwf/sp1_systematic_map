@article{ WOS:000936887500001,
Author = {Cetin, Mahmut and Alsenjar, Omar and Aksu, Hakan and Golpinar, Muhammet
   Said and Akgul, Mehmet Ali},
Title = {Estimation of crop water stress index and leaf area index based on
   remote sensing data},
Journal = {WATER SUPPLY},

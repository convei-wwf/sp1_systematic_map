@article{ WOS:000249622400003,
Author = {Carlson, Kimberly M. and Asner, Gregory P. and Hughes, R. Flint and
   Ostertag, Rebecca and Martin, Roberta E.},
Title = {Hyperspectral remote sensing of canopy biodiversity in Hawaiian lowland
   rainforests},
Journal = {ECOSYSTEMS},

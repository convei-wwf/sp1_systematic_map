@article{ WOS:000317439900036,
Author = {Liu Qing-jie and Jing Lin-hai and Wang Meng-fei and Lin Qi-zhong},
Title = {Hyperspectral Remote Sensing Image Classification Based on SVM Optimized
   by Clonal Selection},
Journal = {SPECTROSCOPY AND SPECTRAL ANALYSIS},

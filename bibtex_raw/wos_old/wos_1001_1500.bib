   and Heil, Petra and Zhao, Jiechen and Zhang, Lin and Cheng, Xiao},
Title = {Satellite-Based Sea Ice Navigation for Prydz Bay, East Antarctica},
Journal = {REMOTE SENSING},
Year = {2017},

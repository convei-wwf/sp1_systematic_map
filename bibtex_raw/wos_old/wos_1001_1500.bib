@inproceedings{ WOS:000438667600026,
Author = {Bardi, Federica and Raspini, Federico and Frodella, William and
   Lombardi, Luca and Nocentini, Massimiliano and Gigli, Giovanni and
   Morelli, Stefano and Corsini, Alessandro and Casagli, Nicola},

@article{ WOS:000470049400017,
Author = {Machwitz, Miriam and Hass, Erik and Junk, Juergen and Udelhoven, Thomas
   and Schlerf, Martin},
Title = {CropGIS - A web application for the spatial and temporal visualization
   of past, present and future crop biomass development},
Journal = {COMPUTERS AND ELECTRONICS IN AGRICULTURE},
Year = {2019},
Volume = {161},
Number = {SI},
Pages = {185-193},

@article{ WOS:000230985600005,
Author = {King, C and Lecomte, V and Le Bissonnais, Y and Baghdadi, N and
   Souchere, V and Cerdan, O},
Title = {Remote-sensing data as an alternative input for the `STREAM' runoff
   model},
Journal = {CATENA},
Year = {2005},
Volume = {62},
Number = {2-3},
Pages = {125-135},

@article{ WOS:000187078500013,
Author = {Seelan, SK and Laguette, S and Casady, GM and Seielstad, GA},
Title = {Remote sensing applications for precision agriculture: A learning
   community approach},
Journal = {REMOTE SENSING OF ENVIRONMENT},
Year = {2003},
Volume = {88},

@article{ WOS:000332183500005,
Author = {Zhang, Penglin and Lv, Zhiyong and Shi, Wenzhong},
Title = {Local Spectrum-Trend Similarity Approach for Detecting Land-Cover Change
   by Using SPOT-5 Satellite Images},

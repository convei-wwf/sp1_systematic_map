@article{ WOS:000445125900018,
Author = {Elhag, Mohamed},
Title = {Understanding of photosynthetically active radiation index under soil
   salinity variation using remote sensing practices in arid environments},
Journal = {DESALINATION AND WATER TREATMENT},

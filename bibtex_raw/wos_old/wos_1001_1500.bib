@article{ WOS:000559302500002,
Author = {Ding, Guoshen and Qiao, Yanli and Yi, Weining and Fang, Wei and Du, Lili},
Title = {Fruit fly optimization algorithm based on a novel fluctuation model and
   its application in band selection for hyperspectral image},
Journal = {JOURNAL OF AMBIENT INTELLIGENCE AND HUMANIZED COMPUTING},
Year = {2021},

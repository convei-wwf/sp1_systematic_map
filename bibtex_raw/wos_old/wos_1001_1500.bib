@article{ WOS:000451733800004,
Author = {Notti, Davide and Giordan, Daniele and Calo, Fabiana and Pepe, Antonio
   and Zucca, Francesco and Pedro Galve, Jorge},
Title = {Potential and Limitations of Open Satellite Data for Flood Mapping},

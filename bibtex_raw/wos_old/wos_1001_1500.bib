Title = {Identification of Groundwater Potential Zones in Langkawi Through Remote
   Sensing and Geographic Information System (GIS) Techniques},
Booktitle = {2020 IEEE 10TH INTERNATIONAL CONFERENCE ON SYSTEM ENGINEERING AND
   TECHNOLOGY (ICSET)},
Series = {International Conference on System Engineering and Technology},
Year = {2020},

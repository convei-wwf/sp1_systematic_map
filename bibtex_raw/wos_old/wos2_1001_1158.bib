@article{ WOS:000224100600005,
Author = {Wimberly, MC and Ohmann, JL},
Title = {A multi-scale assessment of human and environmental constraints on
   forest land cover change on the Oregon (USA) coast range},
Journal = {LANDSCAPE ECOLOGY},
Year = {2004},
Volume = {19},

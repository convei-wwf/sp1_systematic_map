@article{ WOS:000184605000007,
Author = {Sutton, PC},
Title = {A scale-adjusted measure of ``Urban sprawl{''} using nighttime satellite
   imagery},
Journal = {REMOTE SENSING OF ENVIRONMENT},
Year = {2003},

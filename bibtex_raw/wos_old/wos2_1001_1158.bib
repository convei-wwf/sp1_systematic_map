@inproceedings{ WOS:000300440500344,
Author = {Kizuk, R. Alexander},
Editor = {Ongen, DE and Hursen, C and Halat, M and Boz, H},
Title = {Journaling and the Creative Writing Workshop in Counseling and Recovery
   of Adult Participants},
Booktitle = {2ND WORLD CONFERENCE ON PSYCHOLOGY, COUNSELLING AND GUIDANCE-2011},
Series = {Procedia Social and Behavioral Sciences},
Year = {2011},

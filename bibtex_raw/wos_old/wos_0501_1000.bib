Title = {Remote Sensing Image Classification Method Based on Evidence Theory and
   Decision Tree},
Booktitle = {MULTISPECTRAL, HYPERSPECTRAL, AND ULTRASPECTRAL REMOTE SENSING
   TECHNOLOGY, TECHNIQUES, AND APPLICATIONS III},
Series = {Proceedings of SPIE},

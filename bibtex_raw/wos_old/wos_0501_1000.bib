@article{ WOS:000677061700001,
Author = {Gobbo, Stefano and Ghiraldini, Alessandro and Dramis, Andrea and Dal
   Ferro, Nicola and Morari, Francesco},
Title = {Estimation of Hail Damage Using Crop Models and Remote Sensing},
Journal = {REMOTE SENSING},
Year = {2021},

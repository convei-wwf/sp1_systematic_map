Title = {Transferring knowledge from observations and models to decision makers -
   An overview and challenges},
Booktitle = {SENSORS, SYSTEMS AND NEXT-GENERATION SATELLITES VII},
Series = {PROCEEDINGS OF THE SOCIETY OF PHOTO-OPTICAL INSTRUMENTATION ENGINEERS
   (SPIE)},

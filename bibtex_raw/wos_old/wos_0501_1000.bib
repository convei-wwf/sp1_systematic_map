@article{ WOS:000461196000004,
Author = {Phinzi, Kwanele and Ngetar, Njoya Silas},
Title = {The assessment of water-borne erosion at catchment level using GIS-based
   RUSLE and remote sensing: A review},
Journal = {INTERNATIONAL SOIL AND WATER CONSERVATION RESEARCH},

@article{ WOS:000420097000003,
Author = {Karia, Jessica P. and Kiran, G. Sandhya},
Title = {Evaluation of Restoration Potential of Forests Using Remote Sensing \&
   GIS},
Journal = {INTERNATIONAL JOURNAL OF ECOLOGY \& DEVELOPMENT},
Year = {2005},
Volume = {3},
Number = {3},
Pages = {39-54},
Month = {FAL},
Abstract = {Conservation of the forest resources is being rightly considered as an
   urgent task to be pursued throughout the world. The present study
   evaluates the restoration potential of natural forests which is of
   critical importance for reliable and effective decision making for the
   conservation of forest ecosystems. The capability of Remote Sensing and
   GIS technique was used to investigate the forest resources and evaluate
   the restoration potential of the study area. The study was conducted in
   reserve forest areas of Boriad round of Vadodara district, Gujarat,
   India. Considering the contribution of forest cover, soil nutrients,
   plant diversity and dominance values to restoration potential the scores
   were designated to identify most potential areas. The results showed
   that the Boriad round had lowest of 50 per cent to highest of 73 per
   cent of the restoration capability indicating the area to be good
   forests. The method adopted is step towards understanding the ecology by
   studying the vegetation and nutritional dynamics of forests, which will
   form basis for sustainable forest conservation plan.},

@article{ WOS:000373848400007,
Author = {Maheswaran, G. and Selvarani, A. Geetha and Elangovan, K.},
Title = {Groundwater resource exploration in Salem district, Tamil Nadu using GIS
   and remote sensing},
Journal = {JOURNAL OF EARTH SYSTEM SCIENCE},

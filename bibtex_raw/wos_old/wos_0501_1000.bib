@article{ WOS:000475606600009,
Author = {Chave, Jerome and Davies, Stuart J. and Phillips, Oliver L. and Lewis,
   Simon L. and Sist, Plinio and Schepaschenko, Dmitry and Armston, John
   and Baker, Tim R. and Coomes, David and Disney, Mathias and Duncanson,
   Laura and Herault, Bruno and Labriere, Nicolas and Meyer, Victoria and
   Rejou-Mechain, Maxime and Scipal, Klaus and Saatchi, Sassan},
Title = {Ground Data are Essential for Biomass Remote Sensing Missions},

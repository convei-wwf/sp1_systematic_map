@article{ WOS:000447067400008,
Author = {Jo, Hyun-Woo and Kim, Ji-Won and Lim, Chul-Hee and Song, Chol-Ho and
   Lee, Woo-Kyun},
Title = {Optimizing Image Size of Convolutional Neural Networks for Producing
   Remote Sensing-based Thematic Map},
Journal = {KOREAN JOURNAL OF REMOTE SENSING},

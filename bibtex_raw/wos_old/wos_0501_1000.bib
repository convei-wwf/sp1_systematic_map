@article{ WOS:000698773200018,
Author = {Hatfield, Jerry L. and Prueger, John H. and Sauer, Thomas J. and Dold,
   Christian and O'Brien, Peter and Wacha, Ken},
Title = {Applications of Vegetative Indices from Remote Sensing to Agriculture:
   Past and Future},

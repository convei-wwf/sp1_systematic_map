@article{ WOS:000987782400001,
Author = {Huang, Chenhao and Ye, Yang and Jin, Yanhua and Liang, Bangli},
Title = {Research Progress, Hotspots, and Evolution of Nighttime Light Pollution:
   Analysis Based on WOS Database and Remote Sensing Data},
Journal = {REMOTE SENSING},
Year = {2023},

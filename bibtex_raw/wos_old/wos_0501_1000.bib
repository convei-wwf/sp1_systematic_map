@article{ WOS:000251339500013,
Author = {Zhang, Liangpei and Zhong, Yanfei and Huang, Bo and Gong, Jianya and Li,
   Pingxiang},
Title = {Dimensionality reduction based on clonal selection for hyperspectral
   imagery},

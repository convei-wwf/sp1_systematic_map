@inproceedings{ WOS:000342772000043,
Author = {Zhang, Bei and Di, Liping and Yu, Genong and Shao, Yuanzheng and
   Shrestha, Ranjay and Kang, Lingjun},
Book-Group-Author = {IEEE},
Title = {A Web Service Based Application Serving Vegetation Condition Indices for
   Flood Crop Loss Assessment},
Booktitle = {2013 SECOND INTERNATIONAL CONFERENCE ON AGRO-GEOINFORMATICS
   (AGRO-GEOINFORMATICS)},
Series = {International Conference on Agro-Geoinformatics},
Year = {2013},
Pages = {214-219},

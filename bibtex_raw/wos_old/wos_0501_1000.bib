@article{ WOS:000851738400001,
Author = {Chen, Pengchao and Xu, Weicheng and Zhan, Yilong and Yang, Weiguang and
   Wang, Juan and Lan, Yubin},
Title = {Evaluation of Cotton Defoliation Rate and Establishment of Spray
   Prescription Map Using Remote Sensing Imagery},
Journal = {REMOTE SENSING},

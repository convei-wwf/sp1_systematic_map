@article{ WOS:000288727800007,
Author = {Chen, Shuisen and Huang, Wenrui and Chen, Weiqi and Wang, Hongqing},
Title = {Remote sensing analysis of rainstorm effects on sediment concentrations
   in Apalachicola Bay, USA},

Title = {Application of remote sensing data for a wetland ecosystem services
   assessment in the area of Negovan village},
Booktitle = {REMOTE SENSING FOR AGRICULTURE, ECOSYSTEMS, AND HYDROLOGY XX},
Series = {Proceedings of SPIE},
Year = {2018},

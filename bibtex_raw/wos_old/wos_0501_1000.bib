@inproceedings{ WOS:000466548600058,
Author = {Raju, P. L. N. and Gupta, C. and Saikhom, V. and Pandit, S. and Qadir,
   A. and Singh, P. S. and Chutia, D. and Handique, B. K. and Goswami, J.},
Editor = {Kumar, AS and Saran, S and Padalia, H},
Title = {NEW INITIATIVE OF UNMANNED AERIAL VEHICLE (UAV) EMERGING TECHNOLOGY
   APPLICATIONS IN NORTH EAST FOR CAPACITY BUILDING AND OUTREACH ACTIVITIES
   OF NORTH EASTERN SPACE APPLICATIONS CENTRE},
Booktitle = {ISPRS TC V MID-TERM SYMPOSIUM GEOSPATIAL TECHNOLOGY - PIXEL TO PEOPLE},

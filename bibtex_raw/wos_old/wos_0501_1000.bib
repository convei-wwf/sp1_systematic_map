@article{ WOS:000858165500001,
Author = {Luo, Xin and Feng, Qian and Jia, Yuzhen and Chen, Hongyan and Song,
   Yiyun and Xu, Wenbo},
Title = {The Large-Scale Investigation and Analysis of Lophodermium piceae in
   Subalpine Areas Based on Satellite Multi-Spectral Remote Sensing},

@article{ WOS:000287068500006,
Author = {Nakayama, Tadanobu},
Title = {Simulation of the effect of irrigation on the hydrologic cycle in the
   highly cultivated Yellow River Basin},
Journal = {AGRICULTURAL AND FOREST METEOROLOGY},

Title = {Seasonal changes of potential fishing ground formation for Skipjack Tuna
   in the Bone Gulf, Indonesia},
Booktitle = {3RD INTERNATIONAL SYMPOSIUM MARINE AND FISHERIES (ISMF) 2020},
Series = {IOP Conference Series-Earth and Environmental Science},
Year = {2020},

Title = {ANALYSIS OF THE DYNAMICS OF FOREST AREAS IN THE TERRITORIAL
   ADMINISTRATIVE UNITS OF THE APUSENI MOUNTAINS, ROMANIA},
Booktitle = {PUBLIC RECREATION AND LANDSCAPE PROTECTION - WITH SENSE HAND IN HAND...},
Series = {Public Recreation and Landscape Protection},

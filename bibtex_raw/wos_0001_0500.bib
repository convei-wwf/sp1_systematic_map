@article{ WOS:000272023900008,
Author = {Rengers, Norman and Prinz, Torsten},
Title = {JAVA-based Texture Analysis Employing Neighborhood Gray-Tone Difference
   Matrix (NGTDM) for Optimization of Land Use Classifications in High
   Resolution Remote Sensing Data},

@article{ WOS:000273772100032,
Author = {Suri, Sahil and Reinartz, Peter},
Title = {Mutual-Information-Based Registration of TerraSAR-X and Ikonos Imagery
   in Urban Areas},
Journal = {IEEE TRANSACTIONS ON GEOSCIENCE AND REMOTE SENSING},
Year = {2010},

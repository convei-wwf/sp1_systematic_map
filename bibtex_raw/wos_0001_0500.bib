@article{ WOS:000292111800014,
Author = {Zhang, Bing and Sun, Xun and Gao, Lianru and Yang, Lina},
Title = {Endmember Extraction of Hyperspectral Remote Sensing Images Based on the
   Ant Colony Optimization (ACO) Algorithm},

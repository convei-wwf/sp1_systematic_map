@article{ WOS:000486874300111,
Author = {Reyes, Mario Fuentes and Auer, Stefan and Merkle, Nina and Henry,
   Corentin and Schmitt, Michael},
Title = {SAR-to-Optical Image Translation Based on Conditional Generative
   Adversarial Networks-Optimization, Opportunities and Limits},

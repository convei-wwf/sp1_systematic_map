@inproceedings{ WOS:000175365200012,
Author = {Gupta, SC and Goel, RS},
Editor = {Prasad, K and Goel, RS},
Title = {Role of remote sensing in environmental management of hydro-electric
   projects},
Booktitle = {ENVIRONMENTAL MANAGEMENT IN HYDRO ELECTRIC PROJECTS, PROCEEDINGS},
Year = {2000},
Pages = {109-118},
Note = {National Seminar on Environmental Management in Hydro Electric Projects,
   NEW DELHI, INDIA, NOV 11-12, 1999},
Abstract = {Inpursuit of the objective of environmental management of hydro electric
   projects to optimise the benefits and to minimise the adverse impacts on
   ecology, remote sensing has become an effective tool. Space technology
   inputs have been successful in the areas - relevant to storage dam
   projects - like environmental impacts assessment and monitoring, water
   quality mapping, reservoir sedimentation assessment and river
   morphological studies. This advance technique helps to assess the impact
   of submergence of dams by studying the current land use/land cover
   details, soil, watershed characterisation and erosion proneness of
   critical areas. Monitoring of changes on flora, fauna, river morphology,
   afforestation, infrastructure development during and after
   implementation of hydro electric projects using remote sensing
   techniques provide a very strong tool in the hand of engineers and
   scientists. Environmental monitoring of various projects like Idukki
   hydel project, silent valley, Pullichintala and Vamsadhara projects have
   been possible on account of use of satellite data. Remote Sensing
   technique has almost established itself in the application area of
   reservoir sedimentation monitoring, water quality mapping and
   prioritisation of watersheds contributing sediment into the reservoir.
   Monitoring of river behaviour down stream of storage dams has also been
   possible on near real time basis mainly due to inputs available from
   sensing satellites.},

Author = {Heldens, W. and Esch, T. and Asamer, H. and Boettcher, M. and Brito, F.
   and Hirner, A. and Marconcini, M. and Mathot, E. and Metz, A. and
   Permana, H. and Soukop, T. and Stanek, F. and Kuchar, S. and Zeidler, J.
   and Balhar, J.},
Editor = {Erbertseder, T and Chrysoulakis, N and Zhang, Y},
Title = {Exploiting Earth observation data pools for urban analysis: The TEP
   URBAN project},
Booktitle = {REMOTE SENSING TECHNOLOGIES AND APPLICATIONS IN URBAN ENVIRONMENTS II},

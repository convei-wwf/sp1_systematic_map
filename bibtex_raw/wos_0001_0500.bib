Title = {Determining Large Scale Actual Evapotranspiration Using
   Agro-Meteorological and Remote Sensing Data in the Northwest of Sao
   Paulo State, Brazil},
Booktitle = {VII INTERNATIONAL SYMPOSIUM ON IRRIGATION OF HORTICULTURAL CROPS},

@inproceedings{ WOS:000366835103085,
Author = {Ferreira, H. S. and Mustaro, P. N.},
Editor = {Chova, LG and Martinez, AL and Torres, IC},
Title = {REMOTE SENSING TECHNOLOGY ONLINE EDUCATION IN AFRICA: A MULTICULTURAL
   EXPERIENCE},

   machine},
Booktitle = {SENSORS, SYSTEMS, AND NEXT-GENERATION SATELLITES IV},
Series = {PROCEEDINGS OF THE SOCIETY OF PHOTO-OPTICAL INSTRUMENTATION ENGINEERS
   (SPIE)},
Year = {2000},

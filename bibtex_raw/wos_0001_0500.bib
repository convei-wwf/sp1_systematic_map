@article{ WOS:000578443900001,
Author = {Lopez, T. and Al Bitar, A. and Biancamaria, S. and Guentner, A. and
   Jaggi, A.},
Title = {On the Use of Satellite Remote Sensing to Detect Floods and Droughts at
   Large Scales},

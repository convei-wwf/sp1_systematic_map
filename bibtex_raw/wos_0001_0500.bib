@article{ WOS:000629181400027,
Author = {Zhai, Huimin and Xie, Wenquan and Li, Shuqin and Zhang, Qian},
Title = {EVALUATION OF URBAN ECOLOGICAL ENVIRONMENT BASED ON REMOTE SENSING BASED
   ECOLOGICAL INDEX MODEL},
Journal = {FRESENIUS ENVIRONMENTAL BULLETIN},
Year = {2021},
Volume = {30},
Number = {3},
Pages = {2527-2535},
Abstract = {With the development of the society. the relationship between human
   activities and ecological environment becomes increasingly closer, and
   the contradictions of population, resources and environment are
   gradually more obvious. Accurate evaluation of regional ecological
   environment is beneficial to decision makers to learn about the current
   regional ecological environment, and then take proactive measures to
   realize a better regional sustainable development. In order to analyze
   the change characteristics of urban ecological environment, this paper,
   based on the data of remote sensing images from Landsat in 1999 and
   2019. used the principal component analysis (PCA) method to determine
   the weights of four ecological indexes (greenness, dryness, wetness and
   heat), and then selected an evaluation model of remote sensing based
   ecological index (RSEI) to monitor and analyze the ecological
   environment quality of Weinan City from 1999 to 2019. The results showed
   that the mean values of RSEI in Wcinan City increased from 0.489 to
   0.556 during 1999-2019, which indicated the ecological environment
   quality had been improved. The improved area of ecological quality was
   mainly distributed in the central area of Wcinan City and its proportion
   was 49.6\%, While the proportion of ecological environment degradation
   was 15.4\%. and such areas were mainly distributed in some mining areas
   of Han-cheng City and in the southern districts and counties of Weinan
   City (Weinan Section in North Qinling Mountains).},

@article{ WOS:000282075500013,
Author = {Hamel, Melanie A. and Andrefouet, Serge},
Title = {Using very high resolution remote sensing for the management of coral
   reef fisheries: Review and perspectives},
Journal = {MARINE POLLUTION BULLETIN},
Year = {2010},
Volume = {60},
Number = {9},
Pages = {1397-1405},
Month = {SEP},
Abstract = {Coral reef fisheries are critical for food security and as a source of
   income in developing and developed countries, but they are collapsing in
   many areas. Following the emergence and routine availability of
   commercial very high spatial resolution (0.6-10 m) multispectral
   satellite images, we reviewed the use of these new high-quality remote
   sensing data and products for coral reef fisheries management. The
   availability of habitats maps improves management by guiding sampling
   strategies, mapping resources, involving local communities, identifying
   conservation areas, and facilitating Ecosystem Based Fishery Management
   (EBFM) approaches. However, despite their potential, very little use of
   products designed specifically for fishery management can be reported,
   likely due to high costs, inherent technology limitations and lack of

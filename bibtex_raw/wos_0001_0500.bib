@inproceedings{ WOS:000393152800034,
Author = {Hunger, Sebastian and Karrasch, Pierre and Wessollek, Christine},
Editor = {Neale, CMU and Maltese, A},
Title = {Evaluating the potential of image fusion of multispectral and radar
   remote sensing data for the assessment of water body structure},
Booktitle = {REMOTE SENSING FOR AGRICULTURE, ECOSYSTEMS, AND HYDROLOGY XVIII},

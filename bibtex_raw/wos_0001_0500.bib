@article{ WOS:A1992HT77400008,
Author = {GABRYNOWICZ, JI},
Title = {BRINGING SPACE POLICY INTO THE INFORMATION AGE - EARTH OBSERVATIONS AND
   THE CARE ETHIC},
Journal = {SPACE POLICY},
Year = {1992},
Volume = {8},
Number = {2},
Pages = {167-175},
Month = {MAY},
Abstract = {The rapidity of technological development and the end of the Cold War
   have forever changed the geopolitical landscape. The new world in which
   space policy must be made is becoming discernible: a multipolar, fluid
   order that was invisible in the bipolar world of the Cold War, dominated
   as it was by a world view characteristic of the Industrial Age. At the
   same time growing concern about the Earth's environment, with the
   accompanying need for reliable information on which to frame policy, has
   shifted the immediate focus of space missions towards the Earth and the
   advancement of information Age values such as the free flow of
   information, transparent national borders and interdependence. This
   article addresses some of the new challenges space policy makers will
   meet, among them the need to create a coherent policy that reflects the
   Earth-space relationship in space missions. It suggests framing
   remote-sensing data policy by using the concept of relatedness as the
   foundation for near- and long-term Earth-observation goals.},

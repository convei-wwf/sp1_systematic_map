@article{ WOS:000518048500001,
Author = {Virapongse, Arika and Pearlman, Francoise and Pearlman, Jay and
   Murambadoro, Miriam D. and Kuwayama, Yusuke and Glasscoe, Margaret T.},
Title = {Ten rules to increase the societal value of earth observations},

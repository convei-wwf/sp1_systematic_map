Address = {VAN GODEWIJCKSTRAAT 30, 3311 GZ DORDRECHT, NETHERLANDS},
Type = {Article},
Language = {English},
Affiliation = {Asare, R (Corresponding Author), Univ Copenhagen, Dept Geosci \& Nat Resource Management, Copenhagen, Denmark.
   Asare, Richard, Univ Copenhagen, Dept Geosci \& Nat Resource Management, Copenhagen, Denmark.

@article{ WOS:000607921600010,
Author = {Rey-Valette, Helene and Maurel, Pierre and Jabbour, Chady and Cousin,
   Camille and Luque, Sandra and Billaud, Olivier and Salles, Jean Michel},
Title = {Geospatial information contribution to land use planning Evidence from
   land cover and ecosystem services maps},
Journal = {DEVELOPPEMENT DURABLE \& TERRITOIRES},
Year = {2020},
Volume = {11},
Number = {3},
Month = {DEC},
Abstract = {This paper aims to investigate the extent to which ecosystem services
   maps can improve land-use planning decisions by strengthening the
   consideration of environmental issues. The methodology used is based on
   an experimental workshop simulating a participatory decision-making
   process for planning choices by a panel of territorial actors. The
   protocol of the workshop consists in comparing the choices of location
   of new areas to be urbanized based firstly on a fine land cover map and
   secondly, by using a sample of ecosystem services maps. The results show
   the participants' interest in having ecosystem service maps for planning
   decision making. According to them, they bring an added value in terms
   of discovery, coherence, efficiency and acceptability of multifunctional
   space management practices. However, the operational actors of
   territorial engineering still make very little use of these maps derived
   from remote sensing and participatory workshops.},

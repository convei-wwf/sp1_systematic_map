@article{ WOS:000263758500004,
Author = {Mehrabian, Ahmadreza and Naqinezhad, Alireza and Mahiny, Abdolrassoul
   Salman and Mostafavi, Hossein and Liaghati, Homan and Kouchekzadeh,
   Mohsen},
Title = {Vegetation Mapping of the Mond Protected Area of Bushehr Province
   (South-west Iran)},

@article{ WOS:000388798400053,
Author = {Wickramasinghe, Chathura H. and Jones, Simon and Reinke, Karin and
   Wallace, Luke},
Title = {Development of a Multi-Spatial Resolution Approach to the Surveillance
   of Active Fire Lines Using Himawari-8},
Journal = {REMOTE SENSING},

@article{ WOS:000172194700003,
Author = {Khan, MA and Gupta, VP and Moharana, PC},
Title = {Watershed prioritization using remote sensing and geographical
   information system: a case study from Guhiya, India},
Journal = {JOURNAL OF ARID ENVIRONMENTS},
Year = {2001},
Volume = {49},

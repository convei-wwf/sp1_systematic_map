@article{ WOS:000408861800004,
Author = {Chen, Tiantian and Peng, Li and Wang, Qiang and Liu, Shaoquan},
Title = {Measuring the Coordinated Development of Ecological and Economic Systems
   in Hengduan Mountain Area},

@article{ WOS:000390559700004,
Author = {Vogeler, Jody C. and Yang, Zhiqiang and Cohen, Warren B.},
Title = {Mapping Suitable Lewis's Woodpecker Nesting Habitat in a Post-Fire
   Landscape},

Title = {Analysis on the Temporal and Spatial Pattern of Ecosystem Services Based
   on Land-Use Changes:A Case Study of Guanting Reservoir Watershed in
   China},
Booktitle = {ENVIRONMENTAL PROTECTION AND RESOURCES EXPLOITATION, PTS 1-3},
Series = {Advanced Materials Research},

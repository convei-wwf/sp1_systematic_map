@article{ WOS:000400165300006,
Author = {Goncalves, R. M. and Awange, J. L.},
Title = {Three Most Widely Used GNSS-Based Shoreline Monitoring Methods to
   Support Integrated Coastal Zone Management Policies},
Journal = {JOURNAL OF SURVEYING ENGINEERING},
Year = {2017},

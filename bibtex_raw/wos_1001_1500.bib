@article{ WOS:000439347700012,
Author = {Gasc, Amandine and Gottesman, Benjamin L. and Francomano, Dante and
   Jung, Jinha and Durham, Mark and Mateljak, Jason and Pijanowski, Bryan
   C.},
Title = {Soundscapes reveal disturbance impacts: biophonic response to wildfire
   in the Sonoran Desert Sky Islands},

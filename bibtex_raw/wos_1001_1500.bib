Author = {Fanfarillo, E. and Latini, M. and Nicolella, G. and Abbate, G.},
Title = {DEVELOPMENT OF A NEW GIS-BASED METHOD TO DETECT HIGH NATURAL VALUE
   FARMLANDS: A CASE STUDY IN CENTRAL ITALY},
Journal = {ANNALI DI BOTANICA},
Year = {2017},
Volume = {7},
Pages = {25-31},
Abstract = {An original method for the identification of High Natural Value
   farmlands is presented. Gathering information about land use (CORINE
   Land Cover), geomorphology (elevation and Terrain Ruggedness Index) and
   remote sensing data in a GIS environment we were able to develop a new
   detection process; its application to a wide sector of central Italy, in
   areas characterized by high biodiversity and relevant agronomic and
   cultural value, is presented. Thus, a new tool for diminishing sampling
   efforts and economic and time wastes in territorial studies is provided.},

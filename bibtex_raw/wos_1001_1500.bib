@article{ WOS:000255370700007,
Author = {Tomppo, Erkki and Olsson, Hakan and Stahl, Goran and Nilsson, Mats and
   Hagner, Olle and Katila, Matti},
Title = {Combining national forest inventory field plots and remote sensing data
   for forest databases},
Journal = {REMOTE SENSING OF ENVIRONMENT},
Year = {2008},

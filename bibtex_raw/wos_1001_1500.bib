@article{ WOS:000315975900016,
Author = {Kowkabi, L. and Setayesh, Rahman A. R. and Badri, A. and Rajaee, A.},
Title = {The Application of Fuzzy Multi-attribute Group Decision Making to
   Prioritize the Landscapes with high Ecological value: Khoshk River in
   Shiraz},

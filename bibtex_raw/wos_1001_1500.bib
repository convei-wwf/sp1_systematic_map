@article{ WOS:000416220000048,
Author = {Palacios Gonzalez, Mariela and Bonaccorso, Elisa and Papes, Monica},
Title = {Applications of geographic information systems and remote sensing
   techniques to conservation of amphibians in northwestern Ecuador},

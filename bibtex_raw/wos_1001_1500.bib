Editor = {Wibowo, SB and Wicaksono, P},
Title = {Flood control management recommendation of Citarum Hilir Sub-watersheds
   using remote sensing and geographic information system approach},
Booktitle = {SEVENTH GEOINFORMATION SCIENCE SYMPOSIUM 2021},

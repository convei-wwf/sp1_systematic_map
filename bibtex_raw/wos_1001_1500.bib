@inproceedings{ WOS:000241988000028,
Author = {Ardanuy, Philip and Bensman, Ed and Bergen, Bill and Chen, Bob and
   Griffith, Frank and Suttonb, Cary and Hood, Carroll and Ritchie, Adrian
   and Tarro, Andre},

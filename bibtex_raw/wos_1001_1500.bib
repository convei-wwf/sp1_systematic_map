@article{ WOS:000789637200006,
Author = {Karagianni, Aikaterini},
Title = {Road extraction through digital processing and visual interpretation of
   satellite images},
Journal = {INTERNATIONAL JOURNAL OF ENGINEERING AND GEOSCIENCES},

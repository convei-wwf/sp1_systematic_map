@article{ WOS:000977625500001,
Author = {Dai, Shuang and Song, Ke-Fei and Wang, Yan-Long and Zhang, Pei-Jie},
Title = {Two-Dimensional Space Turntable Pitch Axis Trajectory Prediction Method
   Based on Sun Vector and CNN-LSTM Model},

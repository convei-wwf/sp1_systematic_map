Title = {A study on assessment of urbanization and ecosystem changes based on
   MODIS time series in Shanghai municipality from 2000 to 2009},
Booktitle = {REMOTE SENSING AND MODELING OF ECOSYSTEMS FOR SUSTAINABILITY VII},
Series = {Proceedings of SPIE},

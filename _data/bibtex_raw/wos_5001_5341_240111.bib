@inproceedings{ WOS:000341930000191,
Author = {Muramatsu, K. and Masugi, K. and Soyama, N. and Furumi, S. and Daigo, M.},
Editor = {Kajiwara, K and Muramatsu, K and Soyama, N and Endo, T and Ono, A and Akatsuka, S},
Title = {VEGETATION TYPES MAPPING USING ALOS/AVNIR-2 AND PRISM DATA USING
   UNIVERSAL PATTERN DECOMPOSITION METHOD},
Booktitle = {NETWORKING THE WORLD WITH REMOTE SENSING},

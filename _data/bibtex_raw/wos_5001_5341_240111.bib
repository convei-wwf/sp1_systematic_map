Author = {Lin, Ann C. and Salzman, Garrett A. and Bachman, Shelby L. and Burke,
   Rita V. and Zaslow, Tracy and Piasek, Carolina Z. and Edison, Bianca R.
   and Hamilton, Anita and Upperman, Jeffrey S.},
Title = {Assessment of Parental Knowledge and Attitudes Toward Pediatric
   Sports-Related Concussions},
Journal = {SPORTS HEALTH-A MULTIDISCIPLINARY APPROACH},
Year = {2015},

@article{ WOS:000412858600015,
Author = {Huveneers, Charlie and Stehfest, Kilian M. and Simpfendorfer, Colin A.
   and Semmens, Jayson and Hobday, Alistair J. and Pederson, Hugh and
   Stieglitz, Thomas and Vallee, Richard and Webber, Dale and Heupel,

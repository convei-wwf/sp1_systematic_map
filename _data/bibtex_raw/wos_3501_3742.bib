@article{ WOS:000367407000073,
Author = {Bortoleto, Ludmila Araujo and Montagnani Figueira, Cassio Jose and
   Dunning, Jr., John B. and Rodgers, John and da Silva, Alexandre Marco},
Title = {Suitability index for restoration in landscapes: An alternative proposal
   for restoration projects},
Journal = {ECOLOGICAL INDICATORS},

@article{ WOS:000575895200001,
Author = {Brillante, Luca and Martinez-Luscher, Johann and Yu, Runze and Kurtural,
   Sahap Kaan},
Title = {Carbon Isotope Discrimination (δ<SUP>13</SUP>C) of Grape Musts Is a
   Reliable Tool for Zoning and the Physiological Ground-Truthing of Sensor
   Maps in Precision Viticulture},
Journal = {FRONTIERS IN ENVIRONMENTAL SCIENCE},
Year = {2020},
Volume = {8},
Month = {SEP 24},
Abstract = {Carbon stable isotope composition of berry must at harvest (delta C-13)
   is an integrated assessment of plant water status during grape (Vitis
   viniferaL.) berry ripening. Measurement of delta C-13 of grape juice is
   proposed as an alternative to traditional measurements of water status
   to capture the spatial variability of physiological response at the
   vineyard scale, i.e., zoning. We performed samplings at four different
   locations in California, United States, with three different cultivars
   of table and wine grapes (Cabernet Sauvignon, Merlot, Crimson-Seedless).
   Leaf physiology (photosynthesis,A(N), stomatal conductance,g(s)) and
   stem water potentials (psi(stem)) were routinely measured. The delta
   C-13 was measured at harvest and strong relationships were found between
   psi(stem)(R-2= 0.71), stomatal conductance (R-2= 0.71), net carbon
   assimilation (R-2= 0.59) and WUEi (R-2= 0.53). The role of leaf nitrogen
   on the signal was assessed by evaluating relationships between leaf
   nitrogen and WUEi(R-2= 0.54), Ci/Ca (R-2= 0.51), delta C-13 (R-2= 0.44),
   and psi(stem)(R-2= 0.37). Although nitrogen can be among the
   environmental factors able to affect the delta C-13 signal, this
   difference is only observable when variability inNis very large, by
   pooling different vineyards/varieties, but not at the within-vineyard
   scale. The utility of delta C-13 was further tested and measured on
   grape berries sampled on an equidistant grid in a 3.5 ha vineyard where
   psi(stem)was also measured throughout the field season and used to
   delineate management zones. Physiological measurements and grape
   composition were correlated to soil electrical resistivity and
   satellite-derived vegetation index. The two management zones obtained by
   delta C-13 or psi(stem)were spatially similar at 67\% and allowed to
   separate the harvest in two pools having statistically different grape
   composition (soluble solids, organic acids, and anthocyanin profiles).
   Zoning by delta C-13 performed as well as zoning by psi(stem)to separate
   grape phenolic composition, e.g., for selective harvest. Our results
   provided evidence that delta C-13 of grape must is a reliable and
   repeatable assessor of plant water status and gas exchange in vineyard
   systems that are crucial for zoning vineyards, even when irrigated, and
   for ground-truthing sensor maps in precision viticulture.},

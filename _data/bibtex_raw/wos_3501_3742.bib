@article{ WOS:000399851200024,
Author = {Pahlmann, Ingo and Boettcher, Ulf and Kage, Henning},
Title = {Developing and testing an algorithm for site-specific N fertilization of
   winter oilseed rape},
Journal = {COMPUTERS AND ELECTRONICS IN AGRICULTURE},
Year = {2017},

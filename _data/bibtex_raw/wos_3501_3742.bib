@article{ WOS:000474327800012,
Author = {Yourek, Matt and Brooks, Erin S. and Brown, Dave J. and Poggio, Matteo
   and Gasch, Caley},
Title = {Development and application of the soil moisture routing (SMR) model to
   identify subfield-scale hydrologic classes in dryland cropping systems
   using the Budyko framework},

@article{ WOS:000767865500038,
Author = {Wu, Keyu and Wang, Han and Esfahani, Mahdi Abolfazli and Yuan, Shenghai},
Title = {Achieving Real-Time Path Planning in Unknown Environments Through Deep
   Neural Networks},

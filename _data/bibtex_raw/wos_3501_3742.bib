@article{ WOS:001008497700001,
Author = {Baiddah, Abdeslam and Krimissa, Samira and Hajji, Sonia and Ismaili,
   Maryem and Abdelrahman, Kamal and El Bouzekraoui, Meryem and Eloudi,
   Hasna and Elaloui, Abdenbi and Khouz, Abdellah and Badreldin, Nasem and

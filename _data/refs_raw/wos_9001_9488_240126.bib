@article{ WOS:000278909500001,
Author = {Ghosh, Jayanta Kumar and Bhattacharya, Devanjan},
Title = {Knowledge-Based Landslide Susceptibility Zonation System},
Journal = {JOURNAL OF COMPUTING IN CIVIL ENGINEERING},
Year = {2010},

   Herrero, Mario, CSIRO, St Lucia, Qld, Australia.},
DOI = {10.1111/gcb.13995},
ISSN = {1354-1013},
EISSN = {1365-2486},
Keywords = {annual net primary production; forage biomass; global rangeland

@article{ WOS:000247938000002,
Author = {Hunt, Richard H. and Tytgat, Guido N. and Malfertheiner, Peter and Fock,
   Kivong Ming and Heading, Robert C. and Katelaris, Peter H. and McCarthy,
   Denis M. and McColl, Kenneth El. and Moss, Steven F. and Sachs, George
   and Sontag, Stephen J. and Thomson, Allan B. R. and Modlin, Irvin M.},
Title = {Whistler Consensus},
Journal = {JOURNAL OF CLINICAL GASTROENTEROLOGY},

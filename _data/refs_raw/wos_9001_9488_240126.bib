@inproceedings{ WOS:000072955800069,
Author = {Baker, RL and Richardson, JF},
Editor = {Hoots, FR and Kaufman, B and Cefola, PJ and Spencer, DB},
Title = {Space Lift Range System launch vehicle trajectory measurement capability
   Trade Study Results},
Booktitle = {ASTRODYNAMICS 1997},

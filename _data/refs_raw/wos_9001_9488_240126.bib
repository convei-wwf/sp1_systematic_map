@article{ WOS:000080257600001,
Author = {Campbell, NRC and Burgess, E and Choi, BCK and Taylor, C and Wilson, E
   and Cléroux, J and Fodor, JG and Leiter, LA and Spence, D},
Title = {1.: Methods and an overview of the Canadian recommendations},
Journal = {CANADIAN MEDICAL ASSOCIATION JOURNAL},

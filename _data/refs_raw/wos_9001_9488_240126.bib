@article{ WOS:000348744800001,
Author = {Simav, Ozlem and Seker, Dursun Zafer and Tanik, Aysegul},
Title = {POTENTIAL IMPACTS OF SEA LEVEL RISE ON THE COASTAL VULNERABILITY OF
   CUKUROVA DELTA},

@inproceedings{ WOS:000366837206112,
Author = {Hernando, Ana and Legrand, Mathieu and Velazquez, Javier and Herraez,
   Fernando and Mas, Pedro},
Editor = {Chova, LG and Martinez, AL and Torres, IC},
Title = {THREE EDUCATIONAL EXPERIENCES COMBINING B-LEARNING AND M-LEARNING:
   COURSES, SEMINARS AND MASTER CLASSES},

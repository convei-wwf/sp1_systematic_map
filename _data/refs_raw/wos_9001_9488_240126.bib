@article{ WOS:000405382800001,
Author = {Dottori, Francesco and Kalas, Milan and Salamon, Peter and Bianchi,
   Alessandra and Alfieri, Lorenzo and Feyen, Luc},
Title = {An operational procedure for rapid flood risk assessment in Europe},
Journal = {NATURAL HAZARDS AND EARTH SYSTEM SCIENCES},

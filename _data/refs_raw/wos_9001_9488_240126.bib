@inproceedings{ WOS:000256657305030,
Author = {Soria-Ruiz, Jesus and McNairm, Heather},
Book-Group-Author = {IEEE},
Title = {Corn monitoring and crop yield using optical and RADARSAT-2 images},
Booktitle = {IGARSS: 2007 IEEE INTERNATIONAL GEOSCIENCE AND REMOTE SENSING SYMPOSIUM,
   VOLS 1-12: SENSING AND UNDERSTANDING OUR PLANET},
Series = {IEEE International Symposium on Geoscience and Remote Sensing IGARSS},
Year = {2007},
Pages = {3655+},

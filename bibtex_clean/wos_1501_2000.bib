Author = {Mehravar, Soroosh and Amani, Meisam and Moghimi, Armin and Javan, Farzaneh Dadrass and Samadzadegan, Farhad and Ghorbanian, Arsalan and Stein, Alfred and Mohammadzadeh, Ali and Mirmazloumi, S. Mohammad},
Title = {Temperature-Vegetation-soil Moisture-Precipitation Drought Index (TVMPDI); 21-year drought monitoring in Iran using satellite imagery within Google Earth Engine},

@article{ WOS:000483675000007,
Author = {Tesfamariam, Zbelo and Nyssen, Jan and Ghebreyohannes, Tesfaalem and Tafere, Kelemework and Zenebe, Amanuel and Poesen, Jean and Deckers, Seppe and Van Eetvelde, Veerle},
Title = {Transhumance as a driving force of landscape change in the marginal grabens of northern Ethiopia},

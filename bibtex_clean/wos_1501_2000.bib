@article{ WOS:000679026800001,
Author = {Potuckova, Marketa and Kupkova, Lucie and Cervena, Lucie and Lysak, Jakub and Krause, David and Hrazsky, Zaboj and Brezina, Stanislav and Mullerova, Jana},
Title = {Towards resolving conservation issues through historical aerial imagery: vegetation cover changes in the Central European tundra},
Journal = {BIODIVERSITY AND CONSERVATION},

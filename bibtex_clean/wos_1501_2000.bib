@article{ WOS:000549824400051,
Author = {Yu Yue and Hu Xiuqing and Min Min and Xu Tingfa and He Yuqing and Chen Lin and Xu Ronghan},
Title = {NPP/Visible Infrared Image Radiometer Suite Low-Light Image Fusion Algorithm for City Lights in Mid-Eastern China},

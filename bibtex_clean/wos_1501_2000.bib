Author = {Meyer, Jean-Yves and Taureau, Florent and Bisarah, Laetitia and Madi Moussa, Rakamaly and Gorchakova, Elena and Caillaud, Anne},
Title = {Introduced mangroves in the Society Islands, French Polynesia (South Pacific): invasive species or novel ecosystem?},

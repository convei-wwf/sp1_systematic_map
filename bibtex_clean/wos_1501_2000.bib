Title = {Spectral reference data of molecules relevant to Earth's atmosphere: Impact of European metrology research on atmospheric remote sensing},
Booktitle = {REMOTE SENSING OF CLOUDS AND THE ATMOSPHERE XVIII; AND OPTICS IN ATMOSPHERIC PROPAGATION AND ADAPTIVE SYSTEMS XVI},

@article{ WOS:000229775300007,
Author = {Saha, AK and Gupta, RP and Sarkar, I and Arora, MK and Csaplovics, E},
Title = {An approach for GIS-based statistical landslide susceptibility zonation - with a case study in the Himalayas},
Journal = {LANDSLIDES},

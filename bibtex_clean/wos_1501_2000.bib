@inproceedings{ WOS:000664335304121,
Author = {Lin, Chinsu and Ma, Siao-En},
Book-Group-Author = {IEEE},
Title = {MAPPING SURFACE FUEL LOADINGS OF FORESTS USING STRATIFIED RANDOM SAMPLING AND GEOSTATISTICAL ANALYSIS DERIVED DATA},

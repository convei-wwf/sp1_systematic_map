@article{ WOS:001074156500001,
Author = {Zhang, Xuelin and Li, Yuan and Feng, Xiang and Hua, Jian and Yue, Dong and Wang, Jianxiong},
Title = {Application of Multiple-Optimization Filtering Algorithm in Remote Sensing Image Denoising},

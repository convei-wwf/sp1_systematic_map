@article{ WOS:000370969800007,
Author = {Latifi, Hooman and Heurich, Marco and Hartig, Florian and Mueller, Joerg and Krzystek, Peter and Jehl, Hans and Dech, Stefan},
Title = {Estimating over- and understorey canopy density of temperate mixed stands by airborne LiDAR data},

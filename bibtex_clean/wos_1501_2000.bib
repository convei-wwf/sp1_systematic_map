@article{ WOS:000489101500020,
Author = {Boyden, James and Wurm, Penelope and Joyce, Karen E. and Boggs, Guy},
Title = {Spatial Dynamics of Invasive Para Grass on a Monsoonal Floodplain, Kakadu National Park, Northern Australia},

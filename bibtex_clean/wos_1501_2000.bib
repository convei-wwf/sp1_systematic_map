@article{ WOS:000842087400001,
Author = {Lima, Isomar and Sorice Genaro, Andreia Fatima and Loureiro, Geilson and Mattiello-Francisco, Fatima and Robledo Asencio, Jenny Carolina},
Title = {A Framework for Assessing Readiness of Satellite Assembly, Integration and Testing Organization},

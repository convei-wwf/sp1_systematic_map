@article{ WOS:000449284900010,
Author = {Xu, M. and Lacey, C. G. and Armstrong, S. D.},
Title = {The feasibility of satellite remote sensing and spatial interpolation to estimate cover crop biomass and nitrogen uptake in a small watershed},

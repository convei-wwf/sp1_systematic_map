@article{ WOS:000444569000012,
Author = {Mushore, Terence D. and Mutanga, Onisimo and Odindi, John and Dube, Timothy},
Title = {Determining extreme heat vulnerability of Harare Metropolitan City using multispectral remote sensing and socio-economic data},
Journal = {JOURNAL OF SPATIAL SCIENCE},
Year = {2018},
Volume = {63},
Number = {1},
Pages = {173-191},
Abstract = {Urbanisation alters surface landscape characteristics through conversion of natural landscapes to impervious surfaces. Such changes alter the thermal properties of urban landscape mosaics, increasing the urban heat island intensity and the population's vulnerability to heat-related stress. This study aimed at deriving detailed area-specific spatial information on the distribution of heat vulnerability in Harare city, Zimbabwe, valuable for informed urban thermal mitigation, planning and decision-making. Using Landsat-8-derived bio-physical surface properties and socio-demographic factors, findings show that vulnerability to heat-related distress was high in over 40 percent of the city, mainly in densely built-up areas with low-income groups. Comparatively, low to moderate heat vulnerability was observed in the high-income northern suburbs with low physical exposure and population density. Results also showed a strong spatial correlation (=0.61) between heat vulnerability and observed surface temperatures in the hot season, signifying that land surface temperature is a good indicator of heat vulnerability in the area. Furthermore, the study showed that indices derived from moderate-resolution Landsat 8 data improve thermal risk assessment in areas of close proximity. These findings demonstrate the value of readily available multispectral data-sets in determining areas vulnerable to temperature extremes within a heterogeneous urban landscape. The findings are particularly valuable for designing heat-mitigation strategies as well as identifying highly vulnerable areas during heat waves.},
Publisher = {TAYLOR \& FRANCIS LTD},
Address = {2-4 PARK SQUARE, MILTON PARK, ABINGDON OR14 4RN, OXON, ENGLAND},
Type = {Review},
Language = {English},
Affiliation = {Mushore, TD (Corresponding Author), Univ KwaZulu Natal, Sch Agr Earth \& Environm Sci, Discipline Geog, Pietermaritzburg, South Africa. Mushore, Terence D.; Mutanga, Onisimo; Odindi, John, Univ KwaZulu Natal, Sch Agr Earth \& Environm Sci, Discipline Geog, Pietermaritzburg, South Africa. Dube, Timothy, Univ Limpopo, Dept Geog \& Environm Studies, Sovenga, South Africa.},
DOI = {10.1080/14498596.2017.1290558},
ISSN = {1449-8596},
EISSN = {1836-5655},
Keywords = {Land surface temperature; vegetation indices; heat island; vulnerability; heat stress},
Keywords-Plus = {LAND-SURFACE TEMPERATURE; DIFFERENCE WATER INDEX; URBAN AREAS; SOCIAL VULNERABILITY; THERMAL ENVIRONMENT; ABOVEGROUND BIOMASS; IMPERVIOUS SURFACE; SPATIAL-RESOLUTION; CLIMATE-CHANGE; HEALTH-RISK},

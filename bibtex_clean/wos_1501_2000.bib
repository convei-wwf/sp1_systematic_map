@article{ WOS:000484387600074,
Author = {Dymond, John R. and Zorner, Jan and Shepherd, James D. and Wiser, Susan K. and Pairman, David and Sabetizade, Marmar},
Title = {Mapping Physiognomic Types of Indigenous Forest using Space-Borne SAR, Optical Imagery and Air-borne LiDAR},

@article{ WOS:000527895800016,
Author = {Ma, Jinge and Duan, Hongtao and He, Liyuan and Tiffany, Mary and Cao, Zhigang and Qi, Tianci and Shen, Ming and Biggs, Trent and Xu, Xiaofeng},
Title = {Spatiotemporal pattern of gypsum blooms in the Salton Sea, California, during 2000-2018},

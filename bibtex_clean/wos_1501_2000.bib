Type = {Article},
Language = {English},
Affiliation = {Hoskins, AB (Corresponding Author), Mississippi State Univ, Starkville, MS 39762 USA. Hoskins, Aaron B.; Medal, Hugh R., Mississippi State Univ, Starkville, MS 39762 USA.},
DOI = {10.1007/s10479-018-2798-y},

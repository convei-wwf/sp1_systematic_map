@article{ WOS:000869273300002,
Author = {Rockett, Gabriela Camboim and Hesp, Patrick and Portz, Luana and Barboza, Eduardo Guimaraes},
Title = {Aeolian Geodiversity of the Itapeva Dunefield (Brazil) and Geoconservation in the Management of Protected Areas},
Journal = {GEOHERITAGE},

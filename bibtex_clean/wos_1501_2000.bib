@inproceedings{ WOS:000221398500268,
Author = {Bhattacharyya, N and Ray, H},
Book-Group-Author = {IEEE IEEE},
Title = {Design of an embedded receiver for satellite - Based real time voice and data messaging for efficient disaster management},
Booktitle = {IEEE TENCON 2003: CONFERENCE ON CONVERGENT TECHNOLOGIES FOR THE ASIA-PACIFIC REGION, VOLS 1-4},

@inproceedings{ WOS:000189395000063,
Author = {Hwang, JH and Chi, KH},
Editor = {Callaos, N and Lesso, W and Schewe, KD and Atlam, E},
Title = {Design and implementation of environmental/ecology information system using GIS and RS data},
Booktitle = {7TH WORLD MULTICONFERENCE ON SYSTEMICS, CYBERNETICS AND INFORMATICS, VOL XII, PROCEEDINGS: INFORMATION SYSTEMS, TECHNOLOGIES AND APPLICATIONS: II},
Year = {2003},
Pages = {355-360},
Note = {7th World Multiconference on Systemics, Cybernetics and Informatics, ORLANDO, FL, JUL 27-30, 2003},
Organization = {Int Inst Informat \& System},
Abstract = {The twentieth century has certain advantages Such as a higher standard of living for many, while it also has some problems like more polluted environment, the ruin of ecosystem and the devastated nature. Because of this reason, necessity of environment research increases gradually. Therefore in this thesis, suggest an information system for natural environment and ecosystem, also including maps services via internet. And the wide areas of environment narrows down natural environment and ecosystem in this thesis. This thesis represents the integrated 3D DEM using both the process of satellite image and the real value of topographic maps. This DEM is draped on satellite image processed to improve representations of the real world. The 3D visualization and 3D animation with satellite imagery data enables to depict more vivid and realistic world. This thesis also describes and implements the natural environmental/ecological information system that consists of 7 modules to systematically manage environmental data through an enhanced user interface. We use topographic map, satellite imagery data and several thematic maps. Each module has a user interface enabling to assist particular needs of decision-making, for ecological/environmental assessments associated with spatial analysis of ecosystem and classification of the environmental status quo and other purposes. In short, the implemented information system can not only supply environment information to the people through the related institution, the wild-life protection organization and national park but also improves quality of service. In addition, this system will be contributed to more efficient decision-making through the establishment of ecosystem grade.},

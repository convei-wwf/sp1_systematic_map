@article{ WOS:000645139100005,
Author = {Balasubramanian, Banupriya and Ariffin, Azrul Mohd and Mei, Kuan Tze},
Title = {IMPLICATIONS OF GROUND BASED AND SATELLITE DERIVED MEASUREMENTS ON TECHNO-ECONOMIC EVALUATION OF PHOTOVOLTAIC GRID CONNECTED SYSTEM IN KAJANG, MALAYSIA},
Journal = {REVUE ROUMAINE DES SCIENCES TECHNIQUES-SERIE ELECTROTECHNIQUE ET ENERGETIQUE},
Year = {2021},
Volume = {66},

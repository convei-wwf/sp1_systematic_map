@inproceedings{ WOS:000260454800102,
Author = {Zhou, Jianguo and Bai, Tao and Tian, Jiming and Zhang, Aiguang},
Title = {The Study of SVM Optimized by Culture Genetic Algorithm on Predicting Financial Distress},
Booktitle = {PROCEEDINGS OF THE INTERNATIONAL CONFERENCE ON COMPUTER SCIENCE AND INFORMATION TECHNOLOGY},

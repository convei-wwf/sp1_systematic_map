@article{ WOS:000176291500008,
Author = {Komiya, T and Mori, M},
Title = {For clean and safe space environment - Three dimensional standard routing problem},
Journal = {JOURNAL OF THE OPERATIONS RESEARCH SOCIETY OF JAPAN},

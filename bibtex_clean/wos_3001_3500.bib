Author = {Zheng, Xinyu and Wang, Yang and Gan, Muye and Zhang, Jing and Teng, Longmei and Wang, Ke and Shen, Zhangquan and Zhang, Ling},
Title = {Discrimination of Settlement and Industrial Area Using Landscape Metrics in Rural Region},

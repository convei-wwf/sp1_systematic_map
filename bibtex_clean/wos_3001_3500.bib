@article{ WOS:000681434000001,
Author = {Li, Zhaoyang and Xie, Miaomiao and Wang, Huihui and Chen, Bin and Wu, Rongrong and Chen, Yan},
Title = {The spatiotemporal heterogeneity of the relationship between PM<sub>2.5</sub> concentrations and the surface urban heat island effect in Beijing, China},

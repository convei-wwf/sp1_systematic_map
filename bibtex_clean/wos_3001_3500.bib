@article{ WOS:000563090200014,
Author = {Vale, Jones Remo Barbosa and Bordalo, Carlos Alexandre Leao},
Title = {MOPHOMETRIC AND LAND USE AND COVARAGE CHARACTERIZATION OF THE APEU RIVER WATERSHED IN EASTERN AMAZON},
Journal = {REVISTA FORMACAO ONLINE},
Year = {2020},
Volume = {27},

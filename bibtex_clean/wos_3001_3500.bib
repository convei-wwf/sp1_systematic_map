Author = {Chen, Yunzhi and Chen, Wei and Janizadeh, Saeid and Bhunia, Gouri Sankar and Bera, Amit and Quoc Bao Pham and Nguyen Thi Thuy Linh and Balogun, Abdul-Lateef and Wang, Xiaojing},
Title = {Deep learning and boosting framework for piping erosion susceptibility modeling: spatial evaluation of agricultural areas in the semi-arid region},

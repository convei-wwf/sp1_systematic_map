@article{ WOS:000694534300001,
Author = {Li, Yao and Cui, Peng and Ye, Chengming and Marcato Junior, Jose and Zhang, Zhengtao and Guo, Jian and Li, Jonathan},
Title = {Accurate Prediction of Earthquake-Induced Landslides Based on Deep Learning Considering Landslide Source Area},

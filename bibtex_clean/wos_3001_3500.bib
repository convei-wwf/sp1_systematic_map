@article{ WOS:000484189000009,
Author = {Oliveira da Silva, Helen Rose and Pedroso Guimaraes Silva, Siane Cristhina and de Aguiar Cavalcante, Maria Madalena},
Title = {Conservation Unit and Deforestation in the Amazon: Analyses of the State Park in Guajara Mirim in Rondonia/Brazil},

@article{ WOS:000454171400004,
Author = {Okotto-Okotto, Joseph and Raburu, Phillip O. and Obiero, Kevin O. and Obwoyere, Gilbert O. and Mironga, John M. and Okotto, Lorna G. and Raburu, Elizabeth A.},
Title = {Spatio-Temporal Impacts of Lake Victoria Water Level Recession on the Fringing Nyando Wetland, Kenya},

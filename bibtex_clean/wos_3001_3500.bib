@article{ WOS:000320327500017,
Author = {Schofield, Gail and Dimadi, Alexandra and Fossette, Sabrina and Katselidis, Kostas A. and Koutsoubas, Drosos and Lilley, Martin K. S. and Luckman, Adrian and Pantis, John D. and Karagouni, Amalia D. and Hays, Graeme C.},
Title = {Satellite tracking large numbers of individuals to infer population level dispersal and core areas for the protection of an endangered species},

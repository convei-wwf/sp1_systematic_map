@article{ WOS:001049998100030,
Author = {Xu, Qichao and Su, Zhou and Fang, Dongfeng and Wu, Yuan},
Title = {Hierarchical Bandwidth Allocation for Social Community-Oriented Multicast in Space-Air-Ground Integrated Networks},
Journal = {IEEE TRANSACTIONS ON WIRELESS COMMUNICATIONS},

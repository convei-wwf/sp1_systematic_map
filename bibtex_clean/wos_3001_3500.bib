@article{ WOS:000571666200001,
Author = {Oliva, Gabriel and Paredes, Paula and Ferrante, Daniela and Cepeda, Carla and Rabinovich, Jorge},
Title = {Are Patagonia grasslands being overgrazed? A response to Marino et al. (2020)},
Journal = {JOURNAL OF APPLIED ECOLOGY},

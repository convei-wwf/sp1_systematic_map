@article{ WOS:000386493000001,
Author = {Beringer, Jason and Hutley, Lindsay B. and McHugh, Ian and Arndt, Stefan K. and Campbell, David and Cleugh, Helen A. and Cleverly, James and Resco de Dios, Victor and Eamus, Derek and Evans, Bradley and Ewenz, Cacilia and Grace, Peter and Griebel, Anne and Haverd, Vanessa and Hinko-Najera, Nina and Huete, Alfredo and Isaac, Peter and Kanniah, Kasturi and Leuning, Ray and Liddell, Michael J. and Macfarlane, Craig and Meyer, Wayne and Moore, Caitlin and Pendall, Elise and Phillips, Alison and Phillips, Rebecca L. and Prober, Suzanne M. and Restrepo-Coupe, Natalia and Rutledge, Susanna and Schroder, Ivan and Silberstein, Richard and Southall, Patricia and Yee, Mei Sun and Tapper, Nigel J. and van Gorsel, Eva and Vote, Camilla and Walker, Jeff and Wardlaw, Tim},
Title = {An introduction to the Australian and New Zealand flux tower network - OzFlux},

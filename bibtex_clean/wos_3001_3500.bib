@article{ WOS:000348131500012,
Author = {Becker, Douglas A. and Wood, Petra B. and Strager, Michael P. and Mazzarella, Christine},
Title = {Impacts of mountaintop mining on terrestrial ecosystem integrity: identifying landscape thresholds for avian species in the central Appalachians, United States},

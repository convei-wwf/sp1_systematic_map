@article{ WOS:000587856800001,
Author = {Metcalf, Oliver C. and Barlow, Jos and Devenish, Christian and Marsden, Stuart and Berenguer, Erika and Lees, Alexander C.},
Title = {Acoustic indices perform better when applied at ecologically meaningful time and frequency scales},
Journal = {METHODS IN ECOLOGY AND EVOLUTION},
Year = {2021},

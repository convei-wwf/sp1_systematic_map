@article{ WOS:000620114900004,
Author = {McKeon, Thomas P. and Hwang, Wei-Ting and Ding, Zhuoran and Tam, Vicky and Wileyto, Paul and Glanz, Karen and Penning, Trevor M.},
Title = {Environmental exposomics and lung cancer risk assessment in the Philadelphia metropolitan area using ZIP code-level hazard indices},

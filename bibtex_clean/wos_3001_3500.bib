@article{ WOS:000740163300001,
Author = {Zhao, Fubo and Wu, Yiping and Ma, Shuai and Lei, Xiaohui and Liao, Weihong},
Title = {Increased Water Use Efficiency in China and Its Drivers During 2000-2016},
Journal = {ECOSYSTEMS},
Year = {2022},

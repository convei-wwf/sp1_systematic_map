@article{ WOS:000905271600002,
Author = {Salerno, Jonathan and Bailey, Karen and Diem, Jeremy and Konecky, Bronwen and Bridges, Ryan and Namusisi, Shamilah and Bitariho, Robert and Palace, Michael and Hartter, Joel},
Title = {Smallholder Knowledge of Local Climate Conditions Predicts Positive On-Farm Outcomes},

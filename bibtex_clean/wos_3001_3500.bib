@article{ WOS:000351083700043,
Author = {Demir, Gokhan and Aytekin, Mustafa and Akgun, Aykut},
Title = {Landslide susceptibility mapping by frequency ratio and logistic regression methods: an example from Niksar-Resadiye (Tokat, Turkey)},

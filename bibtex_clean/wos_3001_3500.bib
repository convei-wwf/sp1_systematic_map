@article{ WOS:000538763200017,
Author = {Siraw, Zewdu and Bewket, Woldeamlak and Degefu, Mekonnen Adnew},
Title = {Effects of Community-Based Watershed Development on Landscape Greenness and Vegetation Cover in the Northwestern Highlands of Ethiopia},

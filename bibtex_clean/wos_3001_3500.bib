@article{ WOS:000179919800008,
Author = {Vance, C and Geoghegan, J},
Title = {Temporal and spatial modelling of tropical deforestation: a survival analysis linking satellite and household survey data},
Journal = {AGRICULTURAL ECONOMICS},
Year = {2002},
Volume = {27},
Number = {3},

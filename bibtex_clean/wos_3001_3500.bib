@article{ WOS:000364726100017,
Author = {Jung, Richard and Adolph, Winny and Ehlers, Manfred and Farke, Hubert},
Title = {A multi-sensor approach for detecting the different land covers of tidal flats in the German Wadden Sea - A case study at Norderney},

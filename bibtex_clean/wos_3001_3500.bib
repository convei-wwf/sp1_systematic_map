Title = {CUBESAT-DERIVED DETECTION OF SEAGRASSES USING PLANET IMAGERY FOLLOWING UNMIXING-BASED DENOISING: IS SMALL THE NEXT BIG?},
Booktitle = {ISPRS HANNOVER WORKSHOP: HRIGI 17 - CMRT 17 - ISA 17 - EUROCOW 17},
Series = {International Archives of the Photogrammetry, Remote Sensing and Spatial Information Sciences},
Year = {2017},

@article{ WOS:000455338100155,
Author = {Xie, Yaowen and Bie, Qiang and Lu, Hui and He, Lei},
Title = {Spatio-Temporal Changes of Oases in the Hexi Corridor over the Past 30 Years},
Journal = {SUSTAINABILITY},
Year = {2018},
Volume = {10},
Number = {12},
Month = {DEC},
Abstract = {In recent decades, the oases in the Hexi Corridor have gone through a tremendous transformation, which has caused a series of social and environmental problems. We aim to explore quantitatively the characteristics of the oasis expansion and their dynamic mechanism(s) in the Hexi Corridor, and their implications and impact on current and future policies. The spatial distribution pattern and dynamic changes experienced by the oases are examined using Landsat imagery. Their spatio-temporal changes are analyzed using the grid-transformed model and the dynamic-degree model. The model drivers are analyzed based on data from statistics yearbooks and field surveys. The total area of oases in the Hexi Corridor has expanded tremendously during the last 30 years from 10,709 km(2) to 14,950 km(2), almost 40\% of the original value. Oasis evolution patterns of unchanged', expanding', shrinking', and oscillating' are observed at different periods in the three basins. In terms of area, almost half of the oases experienced some change, where most of the changes took place in the ecotone between oases and deserts, and the interior of oases due to the reclamation of abandoned land. Oasis expansion is mainly determined by the human instincts for survival and well-being, which are generally governed by population growth, agricultural policies and economic development. These changes reflect the need to find a balance in the relationship between ecological protection and increasing the well-being of local residents, because unreasonable or excessive development and utilization will cause damage to the local ecological environment.},

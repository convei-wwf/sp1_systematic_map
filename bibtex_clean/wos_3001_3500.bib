@article{ WOS:000278380500006,
Author = {Raymond, Ben and Shaffer, Scott A. and Sokolov, Serguei and Woehler, Eric J. and Costa, Daniel P. and Einoder, Luke and Hindell, Mark and Hosie, Graham and Pinkerton, Matt and Sagar, Paul M. and Scott, Darren and Smith, Adam and Thompson, David R. and Vertigan, Caitlin and Weimerskirch, Henri},

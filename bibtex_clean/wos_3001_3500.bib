@article{ WOS:000247338100001,
Author = {Glenn, Edward P. and Flessa, Karl W. and Cohen, Michael J. and Nagler, Pamela L. and Rowell, Kirsten and Zamora-Arroyo, Francisco},
Title = {Just add water and the Colorado River still reaches the sea},

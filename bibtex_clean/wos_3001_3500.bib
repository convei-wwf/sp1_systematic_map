@article{ WOS:000487233800087,
Author = {Dai, Wen and Hu, Guanghui and Huang, Nan and Zhang, Peng and Yang, Xin and Tang, Guoan},
Title = {A Contour-Directional Detection for Deriving Terrace Ridge From Open Source Images and Digital Elevation Models},

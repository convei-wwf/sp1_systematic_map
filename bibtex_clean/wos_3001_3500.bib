@article{ WOS:000360450500002,
Author = {Watts, Bryan D. and Mojica, Elizabeth K. and Paxton, Barton J.},
Title = {SEASONAL VARIATION IN SPACE USE BY NONBREEDING BALD EAGLES WITHIN THE UPPER CHESAPEAKE BAY},
Journal = {JOURNAL OF RAPTOR RESEARCH},

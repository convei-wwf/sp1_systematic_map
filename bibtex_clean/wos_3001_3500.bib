Author = {Arias, Fernando and Zambrano, Maytee and Broce, Kathia and Medina, Carlos and Pacheco, Hazel and Nunez, Yerenis},
Title = {Hyperspectral imaging for rice cultivation: Applications, methods and challenges},
Journal = {AIMS AGRICULTURE AND FOOD},

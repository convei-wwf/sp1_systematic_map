@article{ WOS:000937766900001,
Author = {Zhao, Jing and Pan, Fangjiang and Xiao, Xiao and Hu, Lianbin and Wang, Xiaoli and Yan, Yu and Zhang, Shuailing and Tian, Bingquan and Yu, Hailin and Lan, Yubin},
Title = {Summer Maize Growth Estimation Based on Near-Surface Multi-Source Data},

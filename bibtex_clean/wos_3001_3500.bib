@article{ WOS:000402012700003,
Author = {Macfarlane, William W. and McGinty, Christopher M. and Laub, Brian G. and Gifford, Suzanne J.},
Title = {High-resolution riparian vegetation mapping to prioritize conservation and restoration in an impaired desert river},

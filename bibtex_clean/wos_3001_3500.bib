@article{ WOS:000247666600005,
Author = {Schofield, Gall and Bishop, Charles M. and MacLean, Grant and Brown, Peter and Baker, Martyn and Katselidis, Kostas A. and Dimopoulos, Panayotis and Pantis, John D. and Hays, Graeme C.},
Title = {Novel GPS tracking of sea turtles as a tool for conservation management},
Journal = {JOURNAL OF EXPERIMENTAL MARINE BIOLOGY AND ECOLOGY},

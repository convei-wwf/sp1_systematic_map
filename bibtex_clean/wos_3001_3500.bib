@article{ WOS:000519135101009,
Author = {Kang, Tingting and Yang, Shuai and Bu, Jingyi and Chen, Jiahao and Gao, Yanchun},
Title = {Quantitative Assessment for the Dynamics of the Main Ecosystem Services and their Interactions in the Northwestern Arid Area, China},

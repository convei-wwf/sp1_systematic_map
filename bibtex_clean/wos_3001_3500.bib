Author = {Safaei, Ali Akbar and Ghassemi, Hassan and Ghiasi, Mahmoud},
Title = {METHODOLOGY OF ACQUIRING VALID DATA BY COMBINING OIL TANKERS' NOON REPORT AND AUTOMATIC IDENTIFICATION SYSTEM SATELLITE DATA},

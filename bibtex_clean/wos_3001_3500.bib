Author = {Tian, Yichao and Huang, Hu and Zhou, Guoqing and Zhang, Qiang and Xie, Xiaokui and Ou, Jinhai and Zhang, Yali and Tao, Jin and Lin, Junliang},
Title = {Mangrove Biodiversity Assessment Using UAV Lidar and Hyperspectral Data in China's Pinglu Canal Estuary},

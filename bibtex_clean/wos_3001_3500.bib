@article{ WOS:000669428400002,
Author = {Sokolowski, Adam and Jankowska, Emilia and Balazy, Piotr and Jedruch, Agnieszka},
Title = {Distribution and extent of benthic habitats in Puck Bay (Gulf of Gdansk, southern Baltic Sea)},

@article{ WOS:000457272500009,
Author = {Ratnayake, R. R. and Karunaratne, S. B. and Lessels, J. S. and Yogenthiran, N. and Rajapaksha, R. P. S. K. and Gnanavelrajah, N.},
Title = {Digital soil mapping of organic carbon concentration in paddy growing soils of Northern Sri Lanka},

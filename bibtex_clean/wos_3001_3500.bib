@article{ WOS:000538695000001,
Author = {Arast, Mina and Ranjbar, Abolfazi and Mousavi, Seyed Hojat and Abdollahi, Khodayar},
Title = {Assessment of the Relationship Between NDVI-Based Actual Evapotranspiration by SEBS},

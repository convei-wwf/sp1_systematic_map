@article{ WOS:000364334500023,
Author = {Li, Jinya and Xu, Bin and Yang, Xiuchun and Jin, Yunxiang and Zhao, Lina and Zhao, Fen and Chen, Shi and Guo, Jian and Qin, Zhihao and Ma, Hailong},
Title = {Characterizing changes in grassland desertification based on Landsat images of the Ongniud and Naiman Banners, Inner Mongolia},

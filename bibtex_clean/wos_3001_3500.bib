@article{ WOS:000247775600013,
Author = {Suryan, Robert M. and Dietrich, Kimberly S. and Melvin, Edward F. and Balogh, Gregory R. and Sato, Fumio and Ozaki, Kiyoaki},
Title = {Migratory routes of short-tailed albatrosses: Use of exclusive economic zones of North Pacific Rim countries and spatial overlap with commercial fisheries in Alaska},

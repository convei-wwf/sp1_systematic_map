@article{ WOS:000377604700007,
Author = {Bian Hongfeng and Yan Tingting and Zhang Zhengxiang and He Chunguang and Sheng Lianxi},
Title = {Mapping Deciduous Broad-leaved Forested Swamps Using ALOS/Palsar Data},

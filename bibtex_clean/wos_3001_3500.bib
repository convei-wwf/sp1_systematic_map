@inproceedings{ WOS:000367945500049,
Author = {Guzzi, Donatella and Pippi, Ivan and Aiazzi, Bruno and Baronti, Stefano and Carla, Roberto and Lastri, Cinzia and Nardino, Vanni and Raimondi, Valentina and Santurri, Leonardo and Selva, Massimo and Alparone, Luciano and Garzelli, Andrea and Lopinto, Ettore and Ananasso, Cristina and Barducci, Alessandro},
Editor = {Meynart, R and Neeck, SP and Shimoda, H and Kimura, T},
Title = {OPTIMA: Advanced methods for the analysis, integration and optimization of PRISMA mission products},
Booktitle = {SENSORS, SYSTEMS, AND NEXT-GENERATION SATELLITES XIX},

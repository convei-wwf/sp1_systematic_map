@article{ WOS:000385488000051,
Author = {Kong, Fanjie and Li, Xiaobing and Wang, Hong and Xie, Dengfeng and Li, Xiang and Bai, Yunxiao},
Title = {Land Cover Classification Based on Fused Data from GF-1 and MODIS NDVI Time Series},

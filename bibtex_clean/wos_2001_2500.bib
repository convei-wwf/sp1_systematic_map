@article{ WOS:000236396100017,
Author = {Rasmussen, HB and Wittemyer, G and Douglas-Hamilton, I},
Title = {Predicting time-specific changes in demographic processes using remote-sensing data},
Journal = {JOURNAL OF APPLIED ECOLOGY},

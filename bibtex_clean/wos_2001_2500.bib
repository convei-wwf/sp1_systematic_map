@article{ WOS:000539535700081,
Author = {Yalcin, Ilyas and Kocaman, Sultan and Gokceoglu, Candan},
Title = {A CitSci Approach for Rapid Earthquake Intensity Mapping: A Case Study from Istanbul (Turkey)},

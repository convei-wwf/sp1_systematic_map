@article{ WOS:000955262800001,
Author = {Yan, Jinfeng and Feng, Pengfei and Jia, Feixue and Su, Fenzhen and Wang, Jing and Wang, Ninghao},
Title = {Identification of secondary functional areas and functional structure analysis based on multisource geographic data},

Author = {Renshaw, Samantha and Hammerschlag, Neil and Gallagher, Austin J. and Lubitz, Nicolas and Sims, David W.},
Title = {Global tracking of shark movements, behaviour and ecology: A review of the renaissance years of satellite tagging studies, 2010-2020},

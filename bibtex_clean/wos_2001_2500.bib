@article{ WOS:000830985500001,
Author = {Chen, Zizi and Small, Gary W.},
Title = {Semantic segmentation of chemical plumes from airborne multispectral infrared images using U-Net},
Journal = {NEURAL COMPUTING \& APPLICATIONS},

@article{ WOS:000521743100062,
Author = {Popescu, Cosmin Alin and Herbei, Mihai Valentin and Sala, Florin},
Title = {REMOTE SENSING IN THE ANALYSIS AND CHARACTERIZATION OF SPATIAL VARIABILITY OF THE TERRITORY. A STUDY CASE IN TIMIS COUNTY, ROMANIA},

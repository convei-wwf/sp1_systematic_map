@article{ WOS:000867948900001,
Author = {Tariq, Aqil and Yan, Jianguo and Ghaffar, Bushra and Qin, Shujing and Mousa, B. G. and Sharifi, Alireza and Huq, Md Enamul and Aslam, Muhammad},
Title = {Flash Flood Susceptibility Assessment and Zonation by Integrating Analytic Hierarchy Process and Frequency Ratio Model with Diverse Spatial Data},
Journal = {WATER},
Year = {2022},

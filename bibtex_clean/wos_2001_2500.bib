@article{ WOS:000460819100031,
Author = {Wu, Jidong and Ye, Mengqi and Wang, Xu and Koks, Elco},
Title = {Building Asset Value Mapping in Support of Flood Risk Assessments: A Case Study of Shanghai, China},

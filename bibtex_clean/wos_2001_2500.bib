@article{ WOS:000494481000002,
Author = {Mu, Guangyi and Chen, Li and Hu, Liangjun and Song, Kaishan},
Title = {Remote Detection of Varying Water Storage in Relation to Surfacial Temperature of Aral Sea},
Journal = {CHINESE GEOGRAPHICAL SCIENCE},

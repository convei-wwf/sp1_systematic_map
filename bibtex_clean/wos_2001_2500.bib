@article{ WOS:000362511400062,
Author = {Helman, David and Lensky, Itamar M. and Tessler, Naama and Osem, Yagil},
Title = {A Phenology-Based Method for Monitoring Woody and Herbaceous Vegetation in Mediterranean Forests from NDVI Time Series},

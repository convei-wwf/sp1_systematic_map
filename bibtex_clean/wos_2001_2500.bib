@article{ WOS:000175207600005,
Author = {Schawe, D and Rohardt, CH and Wichmann, G},
Title = {Aerodynamic design assessment of Strato 2C and its potential for unmanned high altitude airborne platforms},
Journal = {AEROSPACE SCIENCE AND TECHNOLOGY},

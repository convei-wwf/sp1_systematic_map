@article{ WOS:000658891700048,
Author = {Su Jin-tao and Zhang Cheng-xin and Hu Qi-hou and Liu Hao-ran and Liu Jian-guo},
Title = {Analysis of Spatial and Temporal Change Trend of Xinjiang NO<sub>2</sub> in 2007-2017 Based on Satellite Hyperspectral Remote Sensing},

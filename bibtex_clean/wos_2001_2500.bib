@article{ WOS:000358483200023,
Author = {Huang, B. B. and Yan, D. H. and Wang, H.},
Title = {Responses of lakeshore wetlands landscape to water levels changes in Poyang Lake},
Journal = {ENVIRONMENTAL PROGRESS \& SUSTAINABLE ENERGY},

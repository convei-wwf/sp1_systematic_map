@inproceedings{ WOS:000264132900004,
Author = {Sellami, Mounir and Talbi, Mohamed},
Editor = {Marini, A and Talbi, M},
Title = {QUALITY MANAGEMENT IN INFORMATION SYSTEMS FOR COMBATING DESERTIFICATION AND DRY LANDS MANAGEMENT},
Booktitle = {DESERTIFICATION AND RISK ANALYSIS USING HIGH AND MEDIUM RESOLUTION SATELLITE DATA},

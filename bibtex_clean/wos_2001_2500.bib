Title = {Identification of Key Patches for Biodiversity Conservation Based on Possibility of Connectivity Method},
Booktitle = {PROCEEDINGS OF THE 2017 7TH INTERNATIONAL CONFERENCE ON MANUFACTURING SCIENCE AND ENGINEERING (ICMSE 2017)},

@article{ WOS:000402550600002,
Author = {Marais, Eloise A. and Jacob, Daniel J. and Turner, Jay R. and Mickley, Loretta J.},
Title = {Evidence of 1991-2013 decrease of biogenic secondary organic aerosol in response to SO<sub>2</sub> emission controls},

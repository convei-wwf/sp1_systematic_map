@article{ WOS:000207469900016,
Author = {Suarez, Gerardo and van Eck, Torild and Giardini, Domenico and Ahern, Tim and Butler, Rhett and Tsuboi, Seiji},
Title = {The International Federation of Digital Seismograph Networks (FDSN): An Integrated System of Seismological Observatories},

@article{ WOS:000439980200002,
Author = {Huang, Xin and Hu, Ting and Li, Jiayi and Wang, Qing and Benediktsson, Jon Atli},
Title = {Mapping Urban Areas in China Using Multisource Data With a Novel Ensemble SVM Method},

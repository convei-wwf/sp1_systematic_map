@article{ WOS:000431164300020,
Author = {Wang, Litao and Wang, Shixin and Zhou, Yi and Liu, Wenliang and Hou, Yanfang and Zhu, Jinfeng and Wang, Futao},
Title = {Mapping population density in China between 1990 and 2010 using remote sensing},
Journal = {REMOTE SENSING OF ENVIRONMENT},

@article{ WOS:000538075200001,
Author = {Moreira, Luana Lavagnoli and Novais, Rafael Rezende and Schwamback, Dimaghi and de Carvalho Junior, Salomao Martins},
Title = {Spatial-temporal dynamics of rainfall erosivity in the state of Espirito Santo (Brazil) from remote sensing data},

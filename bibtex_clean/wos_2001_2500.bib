@article{ WOS:000355685700008,
Author = {Malahlela, Oupa E. and Cho, Moses A. and Mutanga, Onisimo},
Title = {Mapping the occurrence of <i>Chromolaena odorata</i> (L.) in subtropical forest gaps using environmental and remote sensing data},

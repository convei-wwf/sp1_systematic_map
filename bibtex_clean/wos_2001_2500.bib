@article{ WOS:000527846500005,
Author = {Kumar, A. Senthil and Camacho, Sergio and Searby, Nancy D. and Teuben, Joost and Balogh, Werner},
Title = {Coordinated Capacity Development to Maximize the Contributions of Space Science, Technology, and its Applications in Support of Implementing Global Sustainable Development Agendas-A Conceptual Framework},
Journal = {SPACE POLICY},

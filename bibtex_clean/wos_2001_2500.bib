Author = {Frischauf, Norbert and Horn, Rainer and Kauerhoff, Tilo and Wittig, Manfred and Baumann, Ingo and Pellander, Erik and Koudelka, Otto},
Title = {NewSpace: New Business Models at the Interface of Space and Digital Economy: Chances in an Interconnected World},

@article{ WOS:000455401400014,
Author = {Voda, Mihai and Sarpe, Constantin Adrian and Voda, Anna Izabella},
Title = {Romanian River Basins Lag Time Analysis. The SCS-CN Versus RNS Comparative Approach Developed for Small Watersheds},

@article{ WOS:000889651100001,
Author = {El-Alfy, Muhammad A. and Eissa, Hassan S. and Abd El-Hamid, Hazem T.},
Title = {Environmental and Human Risk Assessment of Radioactive Pollution in Sediments of Northern Nile Delta, Egypt},

@article{ WOS:000479025500009,
Author = {Zhang, Feng and Chen, Yanan and Zhang, Jiquan and Guo, Enliang and Wang, Rui and Li, Danjun},
Title = {Dynamic drought risk assessment for maize based on crop simulation model and multi-source drought indices},

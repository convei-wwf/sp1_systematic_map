Type = {Article},
Language = {English},
Affiliation = {Panthi, S (Corresponding Author), Univ Calif Irvine, Irvine, CA 92697 USA. Panthi, Sunil; Eltawil, Ahmed M., Univ Calif Irvine, Irvine, CA 92697 USA.},
DOI = {10.4018/IJITN.2016070105},
ISSN = {1941-8663},
EISSN = {1941-8671},
Keywords = {Capacity; Dynamic Resource Allocation; Future Satellites; High Throughput Satellite (HTS); Mobility; MultiPort Amplifier (MPA); Satellite Communication},
Research-Areas = {Telecommunications},

@article{ WOS:000477054700030,
Author = {Iwadra, Michael and Odirile, Phillimon T. and Parida, B. P. and Moalafhi, D. B.},
Title = {Evaluation of future climate using SDSM and secondary data (TRMM and NCEP) for poorly gauged catchments of Uganda: the case of Aswa catchment},

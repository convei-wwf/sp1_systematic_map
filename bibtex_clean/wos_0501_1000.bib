@article{ WOS:000415616700065,
Author = {Cuca, Branka and Hadjimitsis, Diofantos G.},
Title = {Space technology meets policy: An overview of Earth Observation sensors for monitoring of cultural landscapes within policy framework for Cultural Heritage},

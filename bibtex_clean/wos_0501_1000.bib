Author = {Maliki, Ali A. A. and Chabuk, Ali and Sultan, Maitham A. and Hashim, Bassim M. and Hussain, Hussain M. and Al-Ansari, Nadhir},
Title = {Estimation of Total Dissolved Solids in Water Bodies by Spectral Indices Case Study: Shatt al-Arab River},

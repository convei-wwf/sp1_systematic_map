@article{ WOS:000373241100127,
Author = {Farrugia, Hazel and Deidun, Alan and Gauci, Adam and Drago, Aldo},
Title = {Defining the Trophic Status of Maltese (Central Mediterranean) Coastal Waters through the Computation of Water Quality Indices Based on Satellite Data},

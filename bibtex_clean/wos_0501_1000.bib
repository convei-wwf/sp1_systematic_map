@article{ WOS:000600770300005,
Author = {Bouguerra, Sana and Jebari, Sihem and Tarhouni, Jamila},
Title = {Spatiotemporal analysis of landscape patterns and its effect on soil loss in the Rmel river basin, Tunisia},

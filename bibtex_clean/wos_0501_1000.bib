@article{ WOS:000661813200185,
Author = {Tangdamrongsub, Natthachet and Hwang, Cheinway and Borak, Jordan S. and Prabnakorn, Saowanit and Han, Jiancheng},
Title = {Optimizing GRACE/GRACE-FO data and <i>a priori</i> hydrological knowledge for improved global terrestial water storage component estimates},
Journal = {JOURNAL OF HYDROLOGY},
Year = {2021},
Volume = {598},
Month = {JUL},
Abstract = {The comprehensive information of global terrestrial water storage (TWS) components (soil moisture, groundwater, snow, surface water) is essential for effective assessment of water resource availability, climate variation, and disaster mitigation measures. Observational data provided by the Gravity Recovery And Climate Experiment (GRACE) and GRACE Follow-On satellite missions offer global TWS variation (Delta TWS) in terms of an integrated water column. However, GRACE spatial resolution is relatively coarse (i.e., 3 degrees), and the vertically integrated value cannot be separated into Delta TWS components directly. This study demonstrates the feasibility to estimate Delta TWS components at any desired spatial-vertical resolution by effectively maintaining the native resolution of the employed hydrological knowledge. It utilizes a least-squares with constraints (LSC) approach to rigorously incorporate GRACE and GRACE-FO data and a priori hydrological knowledge, with the aim to improve global Delta TWS components' accuracy and spatial resolution. The 3 degrees x3 degrees GRACE mascon derived TWS data is disaggregated into the 0.5 degrees x0.5 degrees anomalous soil moisture storage (Delta SMS), groundwater storage (Delta GWS), snow water equivalent (Delta SWE), and surface water storage (Delta SWS) based on the covariance information obtained from the Community Atmosphere Biosphere Land Exchange (CABLE) and the PCRaster Global Water Balance (PCR-GLOBWB) models. Evaluation with different ground measurements and satellite products between 2002 and 2019 exhibits significantly improved accuracy in all individual Delta TWS components. This improvement is of particular note in Delta GWS and Delta SWS, where the LSC approach increases the globally averaged correlation values by approximately 0.13 and 0.05, respectively. Reliable prior knowledge leads to a more accurate Delta TWS component estimate, and the use of ensemble-mean knowledge yields the best result.},

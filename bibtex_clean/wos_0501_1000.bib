@article{ WOS:000468937300005,
Author = {Wang Xuecheng and Yang Fei and Gao Xing and Wang Wei and Zha Xinjie},
Title = {Evaluation of Forest Damaged Area and Severity Caused by Ice-snow Frozen Disasters over Southern China with Remote Sensing},

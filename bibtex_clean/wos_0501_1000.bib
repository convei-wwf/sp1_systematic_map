@article{ WOS:000669227700007,
Author = {Khan, Muhammad Sarfraz and Jeong, Jaehwan and Choi, Minha},
Title = {An improved remote sensing based approach for predicting actual Evapotranspiration by integrating LiDAR},
Journal = {ADVANCES IN SPACE RESEARCH},
Year = {2021},
Volume = {68},

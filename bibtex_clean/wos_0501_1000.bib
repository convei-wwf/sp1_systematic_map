@article{ WOS:000461556000004,
Author = {Rocha Ferreira, Lucianna Marques and Esteves, Luciana S. and de Souza, Enio Pereira and Costa dos Santos, Carlos Antonio},
Title = {Impact of the Urbanisation Process in the Availability of Ecosystem Services in a Tropical Ecotone Area},

@inproceedings{ WOS:000245102300064,
Author = {Thenkabail, Prasad S. and Biradar, Chandrashekhar and Noojipady, Praveen and Islam, Aminul and Vithanage, Jagath and Velpuri, Manohar and Dheeravath, Venkateswarlu and Kulawardhana, Wasantha and Jie, Li Yuan and Gunasinghe, Sarath and Alankara, Ranjith},
Editor = {Li, D and Xia, LY},
Title = {International Water Management Institute's Data Storehouse Pathway (IWMIDSP) - A unique data and knowledge gateway of spatial data with emphasis over river basins},

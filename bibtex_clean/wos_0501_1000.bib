@inproceedings{ WOS:000290114103001,
Author = {Scheffler, C. and Fluegel, W. -A and Krause, P.},
Editor = {Zerger, A and Argent, RM},
Title = {Hydrological Modelling For Quantification Of Coarse Scale Soil Moisture In Southern Africa},
Booktitle = {MODSIM 2005: INTERNATIONAL CONGRESS ON MODELLING AND SIMULATION: ADVANCES AND APPLICATIONS FOR MANAGEMENT AND DECISION MAKING: ADVANCES AND APPLICATIONS FOR MANAGEMENT AND DECISION MAKING},

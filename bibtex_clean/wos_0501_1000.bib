@inproceedings{ WOS:A1997BJ39V00018,
Author = {Singhroy, V},
Editor = {Haskell, G and Rycroft, M},
Title = {Environmental management in developing countries: Remote sensing case studies},
Booktitle = {SPACE OF SERVICE TO HUMANITY: PRESERVING EARTH AND IMPROVING LIFE},
Series = {SPACE STUDIES},

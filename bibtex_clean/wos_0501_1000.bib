@article{ WOS:000395000500005,
Author = {Shahid, Muhammad Adnan and Boccardo, Piero and Usman, Muhammad and Albanese, Adriana and Qamar, Muhammad Uzair},
Title = {Predicting Peak Flows in Real Time through Event Based Hydrologic Modeling for a Trans-Boundary River Catchment},

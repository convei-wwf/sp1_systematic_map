Author = {Wu, Xuejiao and Wang, Xiaoming and Liu, Shiwei and Yang, Yang and Xu, Guobao and Xu, Ying and Jiang, Tong and Xiao, Cunde},
Title = {Snow cover loss compounding the future economic vulnerability of western China},

@article{ WOS:000221193800010,
Author = {Kogan, F and Gitelson, A and Zakarin, E and Spivak, L and Lebed, L},
Title = {AVHRR-based spectral vegetation index for quantitative assessment of vegetation state and productivity: Calibration and validation},
Journal = {PHOTOGRAMMETRIC ENGINEERING AND REMOTE SENSING},

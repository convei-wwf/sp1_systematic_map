@article{ WOS:000701459300001,
Author = {Carpintero, Elisabet and Anderson, Martha C. and Andreu, Ana and Hain, Christopher and Gao, Feng and Kustas, William P. and Gonzalez-Dugo, Maria P.},
Title = {Estimating Evapotranspiration of Mediterranean Oak Savanna at Multiple Temporal and Spatial Resolutions. Implications for Water Resources Management},
Journal = {REMOTE SENSING},
Year = {2021},

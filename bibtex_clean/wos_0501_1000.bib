@article{ WOS:000369399100002,
Author = {Rhoades, Alan M. and Huang, Xingying and Ullrich, Paul A. and Zarzycki, Colin M.},
Title = {Characterizing Sierra Nevada Snowpack Using Variable-Resolution CESM},

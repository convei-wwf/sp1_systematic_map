@article{ WOS:000318621800001,
Author = {Sun, Lina and Lu, Wenxi and Yang, Qingchun and Delgado Martin, Jordi and Li, Di},
Title = {Ecological Compensation Estimation of Soil and Water Conservation Based on Cost-Benefit Analysis},

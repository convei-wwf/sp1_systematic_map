@article{ WOS:000411879600005,
Author = {Zhou Zhongxue and Li Mengtao},
Title = {Spatial-temporal change in urban agricultural land use efficiency from the perspective of agricultural multi-functionality: A case study of the Xi'an metropolitan zone},
Journal = {JOURNAL OF GEOGRAPHICAL SCIENCES},
Year = {2017},

@article{ WOS:000793249100002,
Author = {Gao Bei and Zhang, Shuyu and Guo, Yanlong and Li Yanli and Ning Hu and Jun Liu},
Title = {Study on Meteorological Disaster Monitoring of Field Fruit Industry by Remote Sensing Data},

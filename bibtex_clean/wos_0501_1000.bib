@article{ WOS:000423843100004,
Author = {Sharma, Vinod Kumar and Rao, G. Srinivasa and Bhatt, C. M. and Shukla, Abhinav Kumar and Mishra, Ashish Kumar and Bhanumurthy, V.},
Title = {Automatic procedures analyzing remote sensing data to minimize flood response time: a step towards National flood mapping service},
Journal = {SPATIAL INFORMATION RESEARCH},

@article{ WOS:000087554700003,
Author = {Newson, MD and Newson, CL},
Title = {Geomorphology, ecology and river channel habitat: mesoscale approaches to basin-scale challenges},
Journal = {PROGRESS IN PHYSICAL GEOGRAPHY},
Year = {2000},
Volume = {24},
Number = {2},

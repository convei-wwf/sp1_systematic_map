Author = {Fu, Bolin and Li, Ying and Wang, Yeqiao and Zhang, Bai and Yin, Shubai and Zhu, Honglei and Xing, Zefeng},
Title = {Evaluation of ecosystem service value of riparian zone using land use data from 1986 to 2012},

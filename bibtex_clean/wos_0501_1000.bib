@article{ WOS:000260683500003,
Author = {Randolph, Kaylan and Wilson, Jeff and Tedesco, Lenore and Li, Lin and Pascual, D. Lani and Soyeux, Emmanuel},
Title = {Hyperspectral remote sensing of cyanobacteria in turbid productive water using optically active pigments, chlorophyll <i>a</i> and phycocyanin},

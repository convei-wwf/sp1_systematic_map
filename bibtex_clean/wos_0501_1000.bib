@article{ WOS:000210378000003,
Author = {Ruiz Linares, Jorge and Diaz Lozano, Mauricio and Rachez Tovar, Francoise},
Title = {HOW MUCH DOES A FOREST RESERVE COST?: THE CASE OF EL ROBLEDAL},

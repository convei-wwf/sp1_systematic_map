@article{ WOS:000686757500002,
Author = {Gao, Shengjun and Chen, Yunhao and Li, Kangning and Li, Ying and Yu, Jiaqi and Rao, Ruixuan},
Title = {Mapping Opencast Iron Mine and Mine Solid Waste Based on a New Spectral Index From Medium Spatial Resolution Satellite Data},

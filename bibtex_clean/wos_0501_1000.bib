@article{ WOS:000366185200063,
Author = {Peng, Jian and Niesel, Jonathan and Loew, Alexander and Zhang, Shiqiang and Wang, Jie},
Title = {Evaluation of Satellite and Reanalysis Soil Moisture Products over Southwest China Using Ground-Based Measurements},

@article{ WOS:000488143000015,
Author = {Jung, Hahn Chul and Getirana, Augusto and Arsenault, Kristi R. and Kumar, Sujay and Maigary, Issoufou},
Title = {Improving surface soil moisture estimates in West Africa through GRACE data assimilation},

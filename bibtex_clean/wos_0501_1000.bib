@article{ WOS:000983061900002,
Author = {Veerendra, G. T. N. and Kumaravel, B. and Rao, P. Kodanda Rama and Dey, Subhashish and Manoj, A. V. Phani},
Title = {Forecasting models for surface water quality using predictive analytics},

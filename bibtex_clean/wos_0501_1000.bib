@article{ WOS:000590462700013,
Author = {Liu, Xiaoyin and Xu, Junzeng and Yang, Shihong and Lv, Yuping and Zhuang, Yang},
Title = {Temporal Upscaling of Rice Evapotranspiration Based on Canopy Resistance in a Water-Saving Irrigated Rice Field},

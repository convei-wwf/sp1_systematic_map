@article{ WOS:000336400500012,
Author = {Jiang, Shanhu and Ren, Liliang and Hong, Yang and Yang, Xiaoli and Ma, Mingwei and Zhang, Yu and Yuan, Fei},
Title = {Improvement of Multi-Satellite Real-Time Precipitation Products for Ensemble Streamflow Simulation in a Middle Latitude Basin in South China},

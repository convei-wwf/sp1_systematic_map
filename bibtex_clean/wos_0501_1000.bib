@article{ WOS:000842576800001,
Author = {Safre, Anderson L. S. and Nassar, Ayman and Torres-Rua, Alfonso and Aboutalebi, Mayhar and Saad, Joao C. C. and Manzione, Rodrigo L. and de Castro Teixeira, Antonio Heriberto and Prueger, John H. and McKee, Lynn G. and Alfieri, Joseph G. and Hipps, Lawrence E. and Nieto, Hector and White, William A. and Alsina, Maria del Mar and Sanchez, Luis and Kustas, William P. and Dokoozlian, Nick and Gao, Feng and Anderson, Martha C.},
Title = {Performance of Sentinel-2 SAFER ET model for daily and seasonal estimation of grapevine water consumption},

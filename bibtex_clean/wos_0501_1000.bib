@article{ WOS:000371253100013,
Author = {Moazami, Saber and Golian, Saeed and Hong, Yang and Sheng, Chen and Kavianpour, M. Reza},
Title = {Comprehensive evaluation of four high-resolution satellite precipitation products under diverse climate conditions in Iran},

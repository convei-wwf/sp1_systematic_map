@article{ WOS:000773395700001,
Author = {Ramadhan, Ravidho and Yusnaini, Helmi and Marzuki, Marzuki and Muharsyah, Robi and Suryanto, Wiwit and Sholihun, Sholihun and Vonnisa, Mutya and Harmadi, Harmadi and Ningsih, Ayu Putri and Battaglia, Alessandro and Hashiguchi, Hiroyuki and Tokay, Ali},
Title = {Evaluation of GPM IMERG Performance Using Gauge Data over Indonesian Maritime Continent at Different Time Scales},

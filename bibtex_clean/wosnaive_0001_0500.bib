@article{ WOS:000587168000001,
Author = {Tarolli, Paolo and Pijl, Anton and Cucchiaro, Sara and Wei, Wei},
Title = {Slope instabilities in steep cultivation systems: Process classification and opportunities from remote sensing},
Journal = {LAND DEGRADATION \& DEVELOPMENT},

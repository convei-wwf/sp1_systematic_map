Author = {Vialatte, Aude and Barnaud, Cecile and Blanco, Julien and Ouin, Annie and Choisis, Jean-Philippe and Andrieu, Emilie and Sheeren, David and Ladet, Sylvie and Deconchat, Marc and Clement, Floriane and Esquerre, Diane and Sirami, Clelia},
Title = {A conceptual framework for the governance of multiple ecosystem services in agricultural landscapes},

Title = {Developing and Implementing the Ecosystem Based Multiple Use Forest Management Planning Approach (ETCAP) in Turkey},
Booktitle = {MODELLING, VALUING AND MANAGING MEDITERRANEAN FOREST ECOSYSTEMS FOR NON-TIMBER GOODS AND SERVICES},

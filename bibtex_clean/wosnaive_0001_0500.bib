@article{ WOS:000440265300029,
Author = {Wang, Min and Cao, Wenzhi and Guan, Qingsong and Wu, Gaojie and Wang, Feifei},
Title = {Assessing changes of mangrove forest in a coastal region of southeast China using multi-temporal satellite images},

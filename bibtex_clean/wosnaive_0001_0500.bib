@article{ WOS:000386357500001,
Author = {Wang, Guangyu and Mang, Shari and Cai, Haisheng and Liu, Shirong and Zhang, Zhiqiang and Wang, Liguo and Innes, John L.},
Title = {Integrated watershed management: evolution, development and emerging trends},
Journal = {JOURNAL OF FORESTRY RESEARCH},

@article{ WOS:000959843700001,
Author = {Zhu, Hong and Yao, Jiaqi and Meng, Jian and Cui, Chengling and Wang, Mengyao and Yang, Runlu},
Title = {A Method to Construct an Environmental Vulnerability Model Based on Multi-Source Data to Evaluate the Hazard of Short-Term Precipitation-Induced Flooding},

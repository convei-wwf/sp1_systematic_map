@article{ WOS:000723076700001,
Author = {Islam, Md Monirul and Matsushita, Shusuke and Noguchi, Ryozo and Ahamed, Tofael},
Title = {A damage-based crop insurance system for flash flooding: a satellite remote sensing and econometric approach},

@article{ WOS:000630908700014,
Author = {Kregzdaite, Rusne and Cerneviciute, Jurate and Strazdas, Rolandas},
Title = {EVALUATION OF CULTURAL SECTORS IN EU COUNTRIES},
Journal = {TRANSFORMATIONS IN BUSINESS \& ECONOMICS},
Year = {2020},
Volume = {19},
Number = {3C},
Pages = {618-636},
Abstract = {The cultural and creative industries (CCIs) are becoming important sector for national economies development. It generates considerable economic wealth and acting as catalyst for innovation and progress in other sectors. The CCIs also significantly contributes to the identity, culture and values development. For these reasons, stimulating the development of cultural and creative industries becomes an important challenge for European and national policies. Evaluating the impact of cultural and creative industries on the economic system provides important arguments for policy makers to make strategic decisions in relation to CCIs sector development. The article provides analysis of the current CCIs evaluation methods and indexes such as The Creative City Index, Composite Index of Creative Economy, European Creativity Index, Global Creativity Index, Hong Kong Creativity Index and Creative Space. Based on the analysis strength and weaknesses of these indexes, new analytical tool - Cultural and creative industries index (CCII) was developed and presented in the publication. CCII has been designed exceptionally for the analysis of cultural and economic aspects of creative activities with a focus on cultural and creative industries output factors. The CCII is applied to perform comparative analysis of CCIs sector in EU countries. Results of the analysis is presented in the article.},

@article{ WOS:000896191800001,
Author = {Wang, Shangxiao and Zhang, Ming and Xi, Xi},
Title = {Ecological Environment Evaluation Based on Remote Sensing Ecological Index: A Case Study in East China over the Past 20 Years},

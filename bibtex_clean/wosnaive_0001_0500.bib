@article{ WOS:000361533100007,
Author = {Mi, Nan and Hou, Jingwei and Mi, Wenbao and Song, Naiping},
Title = {Optimal spatial land-use allocation for limited development ecological zones based on the geographic information system and a genetic ant colony algorithm},

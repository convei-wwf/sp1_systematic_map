@article{ WOS:000895120900001,
Author = {Tang, Xianglong and Wei, Tianfeng and He, Yueming and He, Kun},
Title = {Differentiation of Vegetation Community Characteristics by Altitude within Urban Parks and Their Service Functions in a Semi-Arid Mountain Valley: A Case Study of Lanzhou City},

@article{ WOS:000876915700001,
Author = {Ngom, Ndeye Marame and Baratoux, David and Bolay, Matthieu and Dessertine, Anna and Saley, Abdoulatif Abass and Baratoux, Lenka and Mbaye, Modou and Faye, Gayane and Yao, Alphonse Kouakou and Kouame, Kan Jean},
Title = {Artisanal Exploitation of Mineral Resources: Remote Sensing Observations of Environmental Consequences, Social and Ethical Aspects},
Journal = {SURVEYS IN GEOPHYSICS},

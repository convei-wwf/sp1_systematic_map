@article{ WOS:000600185300001,
Author = {Thieme, Alison and Glennie, Erin and Oddo, Perry and McCartney, Sean and Ruid, Madeline and Anand, Anupam},
Title = {Application of Remote Sensing for Ex ante Decision Support and Evaluating Impact},
Journal = {AMERICAN JOURNAL OF EVALUATION},

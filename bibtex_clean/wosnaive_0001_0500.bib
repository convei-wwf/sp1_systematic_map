@inproceedings{ WOS:000453071600052,
Author = {Mendes, S. and Almeida, R. and Duarte, L. and Teodoro, A. C.},
Editor = {Neale, CMU and Maltese, A},
Title = {Remote sensing and GIS combination to evaluate the ecosystems' conditions in ``Serras do Porto{''}},
Booktitle = {REMOTE SENSING FOR AGRICULTURE, ECOSYSTEMS, AND HYDROLOGY XX},

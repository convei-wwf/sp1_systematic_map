@article{ WOS:000648310500001,
Author = {Ellis, Edward A. and Navarro-Martinez, Angelica and Garcia-Ortega, Martha},
Title = {Drivers of forest cover transitions in the Selva Maya, Mexico: Integrating regional and community scales for landscape assessment},

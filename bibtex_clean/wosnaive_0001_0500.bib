@article{ WOS:000472812000024,
Author = {Sannigrahi, Srikanta and Chakraborti, Suman and Joshi, Pawan Kumar and Keesstra, Saskia and Sen, Somnath and Paul, Saikat Kumar and Kreuter, Urs and Sutton, Paul C. and Jha, Shouvik and Dang, Kinh Bac},
Title = {Ecosystem service value assessment of a natural reserve region for strengthening protection and conservation},

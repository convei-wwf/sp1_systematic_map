@article{ WOS:000996642600001,
Author = {Jridi, Linda and Kalaitzidis, Chariton and Alexakis, Dimitrios D.},
Title = {Quantitative Landscape Analysis Using Earth-Observation Data: An Example from Chania, Crete, Greece},

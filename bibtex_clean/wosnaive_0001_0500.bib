@inproceedings{ WOS:000395366400004,
Author = {Aubrecht, Christoph and Koestl, Mario and Steinnocher, Klaus},
Editor = {Tavares, JMRS and Jorge, RMN},
Title = {Population Exposure and Impact Assessment: Benefits of Modeling Urban Land Use in Very High Spatial and Thematic Detail},

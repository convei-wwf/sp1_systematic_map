@article{ WOS:000320871800033,
Author = {Percivall, III, George S. and Alameh, Nadine S. and Caumont, Herve and Moe, Karen L. and Evans, John D.},
Title = {Improving Disaster Management Using Earth Observations-GEOSS and CEOS Activities},

@article{ WOS:000404133200169,
Author = {Tsinganos, Kanaris and Gerasopoulos, Evangelos and Keramitsoglou, Iphigenia and Pirrone, Nicola and ERA-PLANET Team},
Title = {ERA-PLANET, a European Network for Observing Our Changing Planet},

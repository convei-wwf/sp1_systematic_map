Type = {Proceedings Paper},
Language = {English},
Affiliation = {McIntyre, DL (Corresponding Author), Charles Darwin Univ, Key Ctr Trop Wildlife Management, Darwin, NT 0909, Australia. Charles Darwin Univ, Key Ctr Trop Wildlife Management, Darwin, NT 0909, Australia.},

@article{ WOS:000417186100003,
Author = {Fanfarillo, E. and Latini, M. and Nicolella, G. and Abbate, G.},
Title = {DEVELOPMENT OF A NEW GIS-BASED METHOD TO DETECT HIGH NATURAL VALUE FARMLANDS: A CASE STUDY IN CENTRAL ITALY},

@article{ WOS:000341099200003,
Author = {Boissiere, Manuel and Beaudoin, Guillaume and Hofstee, Carola and Rafanoharana, Serge},
Title = {Participating in REDD plus Measurement, Reporting, and Verification (PMRV): Opportunities for Local People?},
Journal = {FORESTS},

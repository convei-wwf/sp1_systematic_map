@article{ WOS:000646866000002,
Author = {Sannigrahi, Srikanta and Chakraborti, Suman and Banerjee, Anushna and Rahmat, Shahid and Bhatt, Sandeep and Jha, Shouvik and Singh, L. K. and Paul, Saikat Kumar and Sen, Somnath},
Title = {Ecosystem service valuation of a natural reserve region for sustainable management of natural resources},

@article{ WOS:000782631000002,
Author = {Mishra, Ashok and Mukherjee, Sourav and Merz, Bruno and Singh, Vijay P. and Wright, Daniel B. and Villarini, Gabriele and Paul, Subir and Kumar, D. Nagesh and Khedun, C. Prakash and Niyogi, Dev and Schumann, Guy and Stedinger, Jery R.},

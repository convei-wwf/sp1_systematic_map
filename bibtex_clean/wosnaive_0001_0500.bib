@article{ WOS:000214891000004,
Author = {Vaz, Eric de Noronha and Caetano, Mario and Nijkamp, Peter},
Title = {Trapped between antiquity and urbanism - a multi-criteria assessment model of the greater Cairo Metropolitan area},

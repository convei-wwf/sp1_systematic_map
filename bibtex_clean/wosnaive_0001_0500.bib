@article{ WOS:000503182000013,
Author = {Deng, Xingdong and Liu, Penghua and Liu, Xiaoping and Wang, Ruoyu and Zhang, Yuanying and He, Jialv and Yao, Yao},
Title = {Geospatial Big Data: New Paradigm of Remote Sensing Applications},

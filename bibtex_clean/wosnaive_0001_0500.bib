@article{ WOS:000993355600001,
Author = {Wang, Qinhua and Chen, Le and Wang, Penglong and Wang, Bao and Yang, Taibao},
Title = {Evolution of County Socio-Ecological Systems in Nature Reserves in Western China Over the Past 30 Years},

@article{ WOS:000412755900012,
Author = {Richards, Daniel R. and Friess, Daniel A.},
Title = {Characterizing Coastal Ecosystem Service Trade-offs with Future Urban Development in a Tropical City},
Journal = {ENVIRONMENTAL MANAGEMENT},

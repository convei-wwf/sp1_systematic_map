@article{ WOS:000473105700184,
Author = {Pijl, Anton and Tosoni, Michele and Roder, Giulia and Sofia, Giulia and Tarolli, Paolo},
Title = {Design of Terrace Drainage Networks Using UAV-Based High-Resolution Topographic Data},

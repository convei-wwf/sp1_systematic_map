@article{ WOS:000427542100093,
Author = {Thompson, Victor D. and DePratter, Chester B. and Lulewicz, Jacob and Lulewicz, Isabelle H. and Thompson, Amanda D. Roberts and Cramb, Justin and Ritchison, Brandon T. and Colvin, Matthew H.},
Title = {The Archaeology and Remote Sensing of Santa Elena's Four Millennia of Occupation},

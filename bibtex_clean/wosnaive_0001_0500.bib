@inproceedings{ WOS:000589976100075,
Author = {Banks, A. C. and Drakopoulos, P. G. and Chaikalis, S. and Spyridakis, N. and Karageorgis, A. P. and Psarra, S. and Taillandier, V and D'Ortenzio, F. and Sofianos, S. and de Madron, Xavier Durrieu},
Editor = {Themistocleous, K and Papadavid, G and Michaelides, S and Ambrosia, V and Hadjimitsis, DG},
Title = {An in situ optical dataset for working towards fiducial reference measurements based satellite ocean colour validation in the Eastern Mediterranean},
Booktitle = {EIGHTH INTERNATIONAL CONFERENCE ON REMOTE SENSING AND GEOINFORMATION OF THE ENVIRONMENT (RSCY2020)},

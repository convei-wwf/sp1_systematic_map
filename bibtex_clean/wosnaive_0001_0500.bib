@article{ WOS:000281393500007,
Author = {Ambastha, K. R. and Hussain, S. A. and Badola, R. and Roy, P. S.},
Title = {Spatial analysis of anthropogenic disturbances in mangrove forests of Bhitarkanika Conservation Area, India},

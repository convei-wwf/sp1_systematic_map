Author = {Li, Ruoxi and Tao, Zui and Zhou, Xiang and Lv, Tingting and Wang, Jin and Xie, Futai and Zhai, Mingjian},
Title = {Data-Driven Selection of Land Product Validation Station Based on Machine Learning},

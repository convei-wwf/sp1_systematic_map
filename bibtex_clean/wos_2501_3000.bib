@article{ WOS:000468328700012,
Author = {Yu, Xue and Li, Yue and Xi, Min and Kong, Fanlong and Pang, Mingyue and Yu, Zhengda},
Title = {Ecological vulnerability analysis of Beidagang National Park, China},

@article{ WOS:000071283300006,
Author = {Gabrynowicz, JI},
Title = {Earth observations: The view from the ground},
Journal = {SPACE POLICY},
Year = {1997},
Volume = {13},
Number = {3},
Pages = {229-244},
Month = {AUG},
Abstract = {A ground-level view of Earth observations reveals that emerging ground segments are of at least two major categories: those that are satellite-driven and which have top-down organizations and those that are data-driven and organized from the bottom up. Or put differently, the former are generally data providers and the latter, generally data users. A ground-level perspective also suggests that continually rising scientific and economic value of data and decreasing national budgets require balancing satellite investment with effective ground segment evolution to provide demonstrable reasons to continue funding both satellites and ground activities. (C) 1997 J. I. Gabrynowicz. Published by Elsevier Science Ltd.},

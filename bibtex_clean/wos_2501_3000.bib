@article{ WOS:000388543600038,
Author = {Liu, Xinyu and Chen, Quanliang and Che, Huizheng and Zhang, Renjian and Gui, Ke and Zhang, Huan and Zhao, Tianliang},
Title = {Spatial distribution and temporal variation of aerosol optical depth in the Sichuan basin, China, the recent ten years},

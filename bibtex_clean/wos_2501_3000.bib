@article{ WOS:000677741800001,
Author = {Chakrabortty, Rabin and Pal, Subodh Chandra and Rezaie, Fatemeh and Arabameri, Alireza and Lee, Saro and Roy, Paramita and Saha, Asish and Chowdhuri, Indrajit and Moayedi, Hossein},
Title = {Flash-flood hazard susceptibility mapping in Kangsabati River Basin, India},

Author = {Miller, Michael R. and Takekawa, John Y. and Battaglia, Daniel S. and Golightly, Richard T. and Perry, William M.},
Title = {SPRING MIGRATION AND SUMMER DESTINATIONS OF NORTHERN PINTAILS FROM THE COAST OF SOUTHERN CALIFORNIA},

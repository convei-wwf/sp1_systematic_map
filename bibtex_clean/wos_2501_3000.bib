Author = {Liu, Yongchao and Li, Jialin and Sun, Chao and Wang, Xinxin and Tian, Peng and Chen, Liumeng and Zhang, Haitao and Yang, Xiaodong and He, Gaili},
Title = {Thirty-year changes of the coastlines, wetlands, and ecosystem services in the Asia major deltas},

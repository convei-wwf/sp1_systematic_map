@article{ WOS:000228504800008,
Author = {Van Tuyl, S and Law, BE and Turner, DP and Gitelman, AI},
Title = {Variability in net primary production and carbon storage in biomass across Oregon forests - an assessment integrating data from forest inventories, intensive sites, and remote sensing},

Author = {AbdelRahman, Mohamed A. E. and Farg, Eslam and Saleh, Ahmed M. and Sayed, Marwa and Abutaleb, Khaled and Arafat, Sayed M. and Elsharkawy, Mohamed M.},
Title = {Mapping of soils and land-related environmental attributes in modern agriculture systems using geomatics},

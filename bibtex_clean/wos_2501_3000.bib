@article{ WOS:000455153300006,
Author = {Zhou Liang and Zhou Chenghu and Yang Fan and Che Lei and Wang Bo and Sun Dongqi},
Title = {Spatio-temporal evolution and the influencing factors of PM<sub>2.5</sub> in China between 2000 and 2015},

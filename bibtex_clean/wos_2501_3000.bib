@article{ WOS:000965403900001,
Author = {Zhang, Zhiru and Wan, Huawei and Peng, Shilei and Huang, Lin},
Title = {Differentiated factors drive the spatial heterogeneity of ecosystem services in Xinjiang Autonomous Region, China},

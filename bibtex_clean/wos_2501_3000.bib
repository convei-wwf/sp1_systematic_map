@article{ WOS:000419892900011,
Author = {Zewdie, Meskerem and Worku, Hailu and Bantider, Amare},
Title = {Temporal Dynamics of the Driving Factors of Urban Landscape Change of Addis Ababa During the Past Three Decades},

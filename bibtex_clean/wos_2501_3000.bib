@article{ WOS:000074499200029,
Author = {Blott, RJ and Wells, NS and Eves, J},
Title = {The STRV 1 microsatellite series: Exploiting the geosynchronous transfer orbit.},
Journal = {ACTA ASTRONAUTICA},
Year = {1997},
Volume = {41},
Number = {4-10},
Pages = {481-491},
Month = {AUG-NOV},

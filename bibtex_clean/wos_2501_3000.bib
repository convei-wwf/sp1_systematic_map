@article{ WOS:000182291900006,
Author = {Burrage, DM and Heron, ML and Hacker, JM and Miller, JL and Stieglitz, TC and Steinberg, CR and Prytz, A},
Title = {Structure and influence of tropical river plumes in the Great Barrier Reef: application and performance of an airborne sea surface salinity mapping system},

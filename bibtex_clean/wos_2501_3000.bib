@article{ WOS:000662311100003,
Author = {Zhang Shuai and Liu Ying and Bate, Bate and Peng Da-lei and Li Can and Zhan Liang-tong},
Title = {Quantitative human risk analysis of 2015 Shenzhen dump failure considering influence of urbanization},
Journal = {JOURNAL OF MOUNTAIN SCIENCE},

@article{ WOS:A1993KW24300011,
Author = {MOELLER, CC and HUH, OK and ROBERTS, HH and GUMLEY, LE and MENZEL, WP},
Title = {RESPONSE OF LOUISIANA COASTAL ENVIRONMENTS TO A COLD-FRONT PASSAGE},

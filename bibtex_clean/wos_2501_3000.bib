@article{ WOS:000459495000002,
Author = {Kouhestani, Fariborz Mansouri and Byrne, James and Johnson, Dan and Spencer, Locke and Hazendonk, Paul and Brown, Bryson},
Title = {Evaluating solar energy technical and economic potential on rooftops in an urban setting: the city of Lethbridge, Canada},

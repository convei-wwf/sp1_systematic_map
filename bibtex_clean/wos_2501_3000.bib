Author = {Holidi and Armanto, M. Edi and Damiri, Nurhayati and Putranto, Dinar Dwi Anugerah},
Title = {Characteristics of Selected Peatland Uses and Soil Moisture Based on TVDI},
Journal = {JOURNAL OF ECOLOGICAL ENGINEERING},

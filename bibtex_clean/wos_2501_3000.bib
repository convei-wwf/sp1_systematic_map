@article{ WOS:000449344000001,
Author = {Nguyen Van Khanh Triet and Nguyen Viet Dung and Merz, Bruno and Apel, Heiko},
Title = {Towards risk-based flood management in highly productive paddy rice cultivation - concept development and application to the Mekong Delta},
Journal = {NATURAL HAZARDS AND EARTH SYSTEM SCIENCES},

@article{ WOS:000456986100004,
Author = {Temgoua, Lucie Felicite and Solefack, Marie Caroline Momo and Voufo, Vianny Nguimdo and Belibi, Chretien Tagne and Tanougong, Armand},
Title = {Spatial and temporal dynamic of land-cover/land-use and carbon stocks in Eastern Cameroon: a case study of the teaching and research forest of the University of Dschang},

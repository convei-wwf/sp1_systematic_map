@article{ WOS:000800229700001,
Author = {Fazelpoor, Khosro and Martinez-fernandez, Vanesa and Yousefi, Saleh and Garcia de Jalon, Diego},
Title = {Increased artificiality trend driven by an inter-basin water transfer on the Zayandeh-rud River floodplain in Iran},

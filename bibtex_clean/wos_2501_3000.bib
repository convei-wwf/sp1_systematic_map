@inproceedings{ WOS:000281054101346,
Author = {Carrer, Dominique and Geiger, Bernhard and Roujean, Jean-Louis and Hautecoeur, Olivier and Cedilnik, Jure and Mahfouf, Jean-Francois and Meurey, Catherine and Franchisteguy, Laurent},

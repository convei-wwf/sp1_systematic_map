@article{ WOS:000463204900003,
Author = {Deines, Jillian M. and Kendall, Anthony D. and Butler, James J. and Hyndman, David W.},
Title = {Quantifying irrigation adaptation strategies in response to stakeholder-driven groundwater management in the US High Plains Aquifer},

Author = {Kansas, John and Vargas, Javier and Skatter, Hans G. and Balicki, Brady and McCullum, Kevin},
Title = {Using Landsat imagery to backcast fire and post-fire residuals in the Boreal Shield of Saskatchewan: implications for woodland caribou management},

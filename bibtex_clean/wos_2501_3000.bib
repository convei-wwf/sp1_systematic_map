@article{ WOS:000389833000003,
Author = {Akhter, Naveed and Sieger, Philipp and Chirico, Francesco},
Title = {IF WE CAN'T HAVE IT, THEN NO ONE SHOULD: SHUTTING DOWN VERSUS SELLING IN FAMILY BUSINESS PORTFOLIOS},
Journal = {STRATEGIC ENTREPRENEURSHIP JOURNAL},

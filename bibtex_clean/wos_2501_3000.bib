Author = {Gomez, Jairo Alejandro and Guan, ChengHe and Tripathy, Pratyush and Duque, Juan Carlos and Passos, Santiago and Keith, Michael and Liu, Jialin},
Title = {Analyzing the Spatiotemporal Uncertainty in Urbanization Predictions},
Journal = {REMOTE SENSING},

@article{ WOS:000879797000006,
Author = {Rivas-Fandino, Paula and Acuna-Alonso, Carolina and Novo, Ana and Leal Pacheco, Fernando Antonio and Alvarez, Xana},
Title = {Assessment of high spatial resolution satellite imagery for monitoring riparian vegetation: riverine management in the smallholding},

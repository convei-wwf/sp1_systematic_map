@article{ WOS:A1993LJ37400004,
Author = {GLATZ, J and VRABLE, DL},
Title = {APPLICATIONS OF ADVANCED COMPOSITES FOR SATELLITE PACKAGING FOR IMPROVED ELECTRONIC-COMPONENT THERMAL MANAGEMENT},

@article{ WOS:000880339400006,
Author = {Qu Lulu and Li Yurui and Wang Yongsheng and Dong Shijie and Wen Qi},
Title = {Dynamic evolution and the mechanism of modern gully agriculture regional function in the Loess Plateau},

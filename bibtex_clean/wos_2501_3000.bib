@article{ WOS:000496269400269,
Author = {Zhou, Min and Wu, Mengquan and Zhang, Guangzong and Zhao, Lianjie and Hou, Xiaoyun and Yang, Yi},
Title = {Analysis of Coastal Zone Data of Northern Yantai Collected by Remote Sensing from 1990 to 2018},

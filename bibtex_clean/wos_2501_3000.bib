@article{ WOS:A1993NE28100001,
Author = {SCHENA, GD},
Title = {ECONOMY-WIDE IMPACT OF A MINERAL PROJECT IN A DEVELOPING-COUNTRY - A GRAPHIC ILLUSTRATION},

@article{ WOS:000875866600001,
Author = {Yang, Xingping and Dai, Xiaoai and Li, Wenyu and Lu, Heng and Liu, Chao and Li, Naiwen and Yang, Zhengli and He, Yuxin and Li, Weile and Fu, Xiao and Ma, Lei and Shan, Yunfeng and Wang, Youlin},
Title = {Socio-Ecological Vulnerability in Aba Prefecture, Western Sichuan Plateau: Evaluation, Driving Forces and Scenario Simulation},

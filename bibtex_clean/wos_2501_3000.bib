@article{ WOS:000328663500024,
Author = {Denis, Gil and Klein, Severine and Gueguen, Berengere},
Title = {Use of social networks for outreach, education and training on space applications: Know-how and experience of Planete Sciences Midi-Pyrenees and CNES},

@article{ WOS:000478203700001,
Author = {Odunga, Pius O. and Manyara, Geoffrey and Yobesia, Mark},
Title = {Estimating the direct contribution of tourism to Rwanda's economy: Tourism satellite account methodology},
Journal = {TOURISM AND HOSPITALITY RESEARCH},

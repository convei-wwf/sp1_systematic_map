@article{ WOS:000581086400001,
Author = {Luo, Weixue and Kim, Hyun Seok and Zhao, Xiuhai and Ryu, Daun and Jung, Ilbin and Cho, Hyunkook and Harris, Nancy and Ghosh, Sayon and Zhang, Chunyu and Liang, Jingjing},
Title = {New forest biomass carbon stock estimates in Northeast Asia based on multisource data},
Journal = {GLOBAL CHANGE BIOLOGY},

Author = {Santos, Maria J. and Khanna, Shruti and Hestir, Erin L. and Andrew, Margaret E. and Rajapakse, Sepalika S. and Greenberg, Jonathan A. and Anderson, Lars W. J. and Ustin, Susan L.},
Title = {Use of Hyperspectral Remote Sensing to Evaluate Efficacy of Aquatic Plant Management},

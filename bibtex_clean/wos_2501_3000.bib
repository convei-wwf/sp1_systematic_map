@article{ WOS:000561346200008,
Author = {Phalke, Aparna R. and Ozdogan, Mutlu and Thenkabail, Prasad S. and Erickson, Tyler and Gorelick, Noel and Yadav, Kamini and Congalton, Russell G.},
Title = {Mapping croplands of Europe, Middle East, Russia, and Central Asia using Landsat, Random Forest, and Google Earth Engine},
Journal = {ISPRS JOURNAL OF PHOTOGRAMMETRY AND REMOTE SENSING},
Year = {2020},

@article{ WOS:000382875600005,
Author = {Omer, Galal and Mutanga, Onisimo and Abdel-Rahman, Elfatih M. and Adam, Elhadi},
Title = {Exploring the utility of the additional WorldView-2 bands and support vector machines in mapping land use/land cover in a fragmented ecosystem, South Africa},

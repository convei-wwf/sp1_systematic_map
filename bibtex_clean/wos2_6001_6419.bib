@article{ WOS:000567458800009,
Author = {Ndehedehe, Christopher E. and Burford, Michele. A. and Stewart-Koster, Ben and Bunn, Stuart E.},
Title = {Satellite-derived changes in floodplain productivity and freshwater habitats in northern Australia (1991-2019)},

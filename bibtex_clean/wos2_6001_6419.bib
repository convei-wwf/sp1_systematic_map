@article{ WOS:000513416300001,
Author = {Hope, Julia and Quinlan, Kathleen M.},
Title = {Staying local: how mature, working-class students on a satellite campus leverage community cultural wealth},
Journal = {STUDIES IN HIGHER EDUCATION},
Year = {2021},
Volume = {46},

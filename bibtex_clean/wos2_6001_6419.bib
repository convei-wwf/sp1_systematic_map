@article{ WOS:000187883700004,
Author = {Towns, DR and Broome, KG},
Title = {From small Maria to massive Campbell: forty years of rat eradications from New Zealand islands},
Journal = {NEW ZEALAND JOURNAL OF ZOOLOGY},

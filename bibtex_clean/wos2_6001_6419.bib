Title = {Application of virtual reality GIS in urban planning - an example in Huangdao district},
Booktitle = {GEOINFORMATICS 2007: GEOSPATIAL INFORMATION TECHNOLOGY AND APPLICATIONS, PTS 1 AND 2},

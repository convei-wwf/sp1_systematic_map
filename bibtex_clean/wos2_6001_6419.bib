@article{ WOS:000830282500001,
Author = {Rendana, Muhammad and Idris, Wan Mohd Razi and Rahim, Sahibin Abdul},
Title = {Changes in air quality during and after large-scale social restriction periods in Jakarta city, Indonesia},

@article{ WOS:000690582400001,
Author = {Veldtman, Ruan and Daly, Derek and Bekker, Gerard F. H. V. G.},
Title = {Spatio-Environmental Analysis of Vespula germanica Nest Records Explains Slow Invasion in South Africa},

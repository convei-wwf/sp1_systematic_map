Author = {Amgoth, Ashok and Rani, Hari Ponnamma and Jayakumar, K. V.},
Title = {Exploring LULC changes in Pakhal Lake area, Telangana, India using QGIS MOLUSCE plugin},
Journal = {SPATIAL INFORMATION RESEARCH},

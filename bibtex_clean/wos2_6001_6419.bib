Title = {Analysis and optimization of spectrum sharing in IMT-2020 and satellite systems considering harmful interferences},
Booktitle = {INTERNATIONAL CONFERENCE ON ELECTRICAL, COMPUTER AND ENERGY TECHNOLOGIES (ICECET 2021)},
Year = {2021},

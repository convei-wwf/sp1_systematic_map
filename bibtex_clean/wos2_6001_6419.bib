@article{ WOS:000390824700019,
Author = {Lipinski, Victor Mendes and dos Santos, Tiago Gomes and Schuch, Andre Passaglia},
Title = {An UV-sensitive anuran species as an indicator of environmental quality of the Southern Atlantic Rainforest},

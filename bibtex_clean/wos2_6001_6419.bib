@article{ WOS:000633123000001,
Author = {Tariq, Muhammad Atiq Ur Rehman and Rajabi, Zohreh and Muttil, Nitin},
Title = {An Evaluation of Risk-Based Agricultural Land-Use Adjustments under a Flood Management Strategy in a Floodplain},

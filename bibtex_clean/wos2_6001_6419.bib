@article{ WOS:000695408800003,
Author = {Wang, Yaoping and Mao, Jiafu and Jin, Mingzhou and Hoffman, Forrest M. and Shi, Xiaoying and Wullschleger, Stan D. and Dai, Yongjiu},
Title = {Development of observation-based global multilayer soil moisture products for 1970 to 2016},

@article{ WOS:000691547700001,
Author = {Rashidzadeh, Ehsan and Hadji Molana, Seyyed Mohammad and Soltani, Roya and Hafezalkotob, Ashkan},
Title = {Assessing the sustainability of using drone technology for last-mile delivery in a blood supply chain},

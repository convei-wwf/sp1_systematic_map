@article{ WOS:000404781800005,
Author = {Roslee, Rodeano and Jamaludin, Tajul Anuar and Simon, Norbert},
Title = {Landslide Vulnerability Assessment (LVAs): A Case Study from Kota Kinabalu, Sabah, Malaysia},

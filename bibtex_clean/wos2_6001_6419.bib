@inproceedings{ WOS:000332043900084,
Author = {Farah Simon, M. I. Lisette and Adam Siade, Juan Alberto and Santillan Gutierrez, Saul Daniel and Romo Fuentes, Carlos and Ramirez Aguilar, Alberto},

@article{ WOS:000840052200001,
Author = {Lopez-Caloca, Alejandra A. and Monsivais-Huertero, Alejandro and Lopez-Amaya, Jubal},
Title = {Sentinel-1 observation for shoreline delineation applied to Mexico's Coast},
Journal = {GEOCARTO INTERNATIONAL},
Year = {2022},
Volume = {37},

@article{ WOS:000442986600009,
Author = {Lastauskas, Povilas and Stakenas, Julius},
Title = {OPENNESS AND STRUCTURAL LABOR MARKET REFORMS: EX ANTE COUNTERFACTUALS},
Journal = {CONTEMPORARY ECONOMIC POLICY},
